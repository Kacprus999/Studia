@article{ WOS:000405424100056,
Author = {Wang, Yanli and Ding, Yi},
Title = {{Structural stability and the electronic properties of a (SiH)(2)O-formed
   siloxene sheet: a computational study}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
Year = {{2017}},
Volume = {{19}},
Number = {{27}},
Pages = {{18030-18035}},
Month = {{JUL 21}},
Abstract = {{Very recently, a (SiH)(2)O-formed siloxene sheet has been synthesized in
   an experiment {[}S. Li, et al. J. Mater. Chem. A 2016, 4, 15841].
   Utilizing first-principles calculations, we have systematically
   investigated the structural stability and the physical properties of
   this (SiH)(2)O sheet. We have found a dynamically stable structure
   because it is a puckered one with bent Si-O-Si connections, which leads
   to an anisotropic elastic behaviour with negative Poisson ratios. The
   (SiH)(2)O sheet has a strain-tunable direct band gap whose valence and
   conduction band edges straddle the redox potentials of water even in the
   presence of stress (either tensile or compressive). The calculated
   carrier mobilities are prominently anisotropic: in its zigzag direction
   the electron mobility reaches a high value of 5.39 x 10(3) cm(2) V-1
   s(-1), while the hole mobility remains small. These different carrier
   mobilities permit fast charge separation and transfer, which is crucial
   for efficient solar-to-electric energy conversion for photovoltaic and
   water splitting applications. Similar to phosphorene, a typical linear
   dichroism phenomenon is also present in the (SiH)(2)O sheet. Due to its
   outstanding electronic properties, the (SiH)(2)O sheet is a promising
   material in the fields of nano-electronics, sustainable energy and fuel
   generation.}},
