@article{ WOS:000397619900001,
Author = {Deng, Binghui and Hou, Jie and Zhu, Hanxing and Liu, Sheng and Liu,
   Emily and Shi, Yunfeng and Peng, Qing},
Title = {{The normal-auxeticity mechanical phase transition in graphene}},
Journal = {{2D MATERIALS}},
Year = {{2017}},
Volume = {{4}},
Number = {{2}},
Month = {{JUN}},
Abstract = {{When a solid object is stretched, in general, it shrinks transversely.
   However, the abnormal ones are auxetic, which exhibit lateral expansion,
   or negative Poisson ratio. While graphene is a paradigm 2D material,
   surprisingly, graphene converts from normal to auxetic at certain
   strains. Here, we show via molecular dynamics simulations that the
   normal-auxeticity mechanical phase transition only occurs in uniaxial
   tension along the armchair direction or the nearest neighbor direction.
   Such a characteristic persists at temperatures up to 2400 K. Besides
   monolayer, bilayer and multi-layer graphene also possess such a
   normal-auxeticity transition. This unique property could extend the
   applications of graphene to new horizons.}},
