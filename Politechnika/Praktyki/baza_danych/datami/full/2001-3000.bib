@article{ WOS:000376706500005,
Author = {Cabras, Luigi and Brun, Michele},
Title = {{A class of auxetic three-dimensional lattices}},
Journal = {{JOURNAL OF THE MECHANICS AND PHYSICS OF SOLIDS}},
Year = {{2016}},
Volume = {{91}},
Pages = {{56-72}},
Month = {{JUN}},
Abstract = {{We propose a class of auxetic three-dimensional lattice structures. The
   elastic microstructure can be designed to have an omnidirectional
   Poisson's ratio arbitrarily close to the stability limit of -1. The
   cubic behaviour of the periodic system has been fully characterized; the
   minimum and maximum Poisson's ratio and the associated principal
   directions are given as a function of the microstructural parameters.
   The initial microstructure is then modified into a body-centred cubic
   system that can achieve Poisson's ratio lower than -1 and that can also
   behave as an isotropic three-dimensional auxetic structure. (C) 2016
   Elsevier Ltd. All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
