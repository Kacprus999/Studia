@article{ WOS:000351400200008,
Author = {Rushchitsky, Jeremiah J.},
Title = {{Auxetic linearly elastic isotropic materials: restrictions on elastic
   moduli}},
Journal = {{ARCHIVE OF APPLIED MECHANICS}},
Year = {{2015}},
Volume = {{85}},
Number = {{4}},
Pages = {{517-522}},
Month = {{APR}},
Abstract = {{A necessity is shown for new reasoning of procedures for estimating the
   values of elastic moduli of the classical linear isotropic theory of
   elasticity, in which an existence of the materials with negative values
   of the Poisson ratio (auxetic materials) is allowed. The new estimates
   of values of elastic moduli are offered, which are based on only the
   necessary condition of positivity of the internal energy. It is proven
   that this condition allows an existence of arbitrary negative values of
   Poisson ratio sigma, which is accompanied by the negative values of the
   Lame modulus lambda as well as the bulk modulus k and Young modulus E,
   just as the new mechanical effects-expansion of the standard sample rod
   under unilateral tension and expansion of the standard sample cube under
   hydrostatic pressure.}},
