@article{ WOS:000384003000013,
Author = {Jiang, Wei and Ma, Hua and Feng, Mingde and Yan, Leilei and Wang, Jiafu
   and Wang, Jun and Qu, Shaobo},
Title = {{Origami-inspired building block and parametric design for mechanical
   metamaterials}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{2016}},
Volume = {{49}},
Number = {{31}},
Month = {{AUG 10}},
Abstract = {{An origami-based building block of mechanical metamaterials is proposed
   and explained by introducing a mechanism model based on its geometry.
   According to our model, this origami mechanism supports response to
   uniaxial tension that depends on structure parameters. Hence, its
   mechanical properties can be tunable by adjusting the structure
   parameters. Experiments for poly lactic acid (PLA) samples were carried
   out, and the results are in good agreement with those of finite element
   analysis (FEA). This work may be useful for designing building blocks of
   mechanical metamaterials or other complex mechanical structures.}},
