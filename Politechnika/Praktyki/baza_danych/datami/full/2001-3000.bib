@article{ WOS:000362668400008,
Author = {Gupta, Arti and Tandon, R. P. and Shinde, A. B. and Krishna, P. S. R.
   and Chatterjee, Ratnamala},
Title = {{Negative spontaneous magnetization and semi-spin glass magnetic order in
   mixed spinel Co0.6Zn0.4Fe1.7Mn0.3O4}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2015}},
Volume = {{118}},
Number = {{13}},
Month = {{OCT 7}},
Abstract = {{In this paper, we establish the negative spontaneous magnetization in Mn
   and Zn substituted cobalt ferrite Co0.6Zn0.4Fe1.7Mn0.3O4 (CZFMO). It is
   suggested that the origin of negative spontaneous magnetization is due
   to the substitution of small sized Mn+4 ions (compared to Fe+3 ions) at
   the octahedral B site in compound Co0.6Zn0.4 Fe2O4. The low value of
   Poisson's ratio similar to 0.202 for this compound possibly contributes
   towards the easy distortion in the bond length and bond angle, causing
   increase in Fe-O bond distance/decrease in Fe-O-Fe bond angle with Mn
   substitution, leading to considerably weak Fe-O-Fe superexchange
   interaction at the octahedral B site. The neutron diffraction data
   clearly illustrated the significant reduction in ordered magnetic moment
   at the B site, with the resultant negative spontaneous magnetization (M
   = M-B - M-A) in this mixed spinel system. The spin disorder also gives
   rise to an interesting semi-spin glass behavior in CZFMO. (C) 2015 AIP
   Publishing LLC.}},
