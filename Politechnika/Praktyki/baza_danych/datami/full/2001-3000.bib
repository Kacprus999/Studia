@article{ WOS:000383602900012,
Author = {Narojczyk, Jakub W. and Kowalik, Mikolaj and Wojciechowski, Krzysztof W.},
Title = {{Influence of nanochannels on Poisson's ratio of degenerate crystal of
   hard dimers}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2016}},
Volume = {{253}},
Number = {{7}},
Pages = {{1324-1330}},
Month = {{JUL}},
Abstract = {{Hard-body models reproduce many fundamental features of real physical
   systems and are convenient tools to study the role of molecular shape
   and its influence on macroscopic, for example, elastic, properties of
   condensed matter. In this article, a degenerate (aperiodic) crystal of
   hard dimers with arrays of nanochannels introduced in the direction
   {[}001] is studied by Monte Carlo simulations. The nanochannels are
   filled by hard spheres of diameter sigma' which can be different from
   the diameter sigma of spheres forming dimers. It is shown that, by
   modifying the ratio o'/o, one can qualitatively modify the Poisson's
   ratio of the system. In particular, one can obtain partial auxetics
   (i.e., systems with negative Poisson's ratio in some directions) with
   auxetic directions {[}110] or {[}111]. The applied method is convenient
   to determine Poisson's ratio in all directions for crystals of arbitrary
   symmetry. Explicit formulae for Poisson's ratio in main directions of
   the studied structures are given. (C) 2016 WILEY-VCH Verlag GmbH \& Co.
   KGaA, Weinheim}},
