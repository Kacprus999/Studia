@article{ WOS:000270018600011,
Author = {Chirima, Gleny T. and Zied, Khaled M. and Ravirala, Naveen and Alderson,
   Kim L. and Alderson, Andrew},
Title = {{Numerical and analytical modelling of multi-layer adhesive-film
   interface systems}},
