@article{ WOS:000306278700022,
Author = {Topolov, Vitaly Yu and Krivoruchko, Andrey V. and Bowen, Christopher R.},
Title = {{Anisotropy of electromechanical properties and hydrostatic response of
   advanced 2-2-type composites}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLICATIONS AND MATERIALS SCIENCE}},
Year = {{2012}},
Volume = {{209}},
Number = {{7}},
Pages = {{1334-1342}},
Month = {{JUL}},
Abstract = {{The paper reports results on the high performance of novel piezo-active
   composites based on relaxor-ferroelectric single crystals (SCs) of
   xPb(In0.5Nb0.5)O3yPb(Mg1/3Nb2/3)O3(1-x-y)PbTiO3. We present a
   comparative analysis of the electromechanical properties of 22-type
   composites based on single-domain SCs from symmetry classes 3m and mm2.
   The parallel connection of the layers, the presence of an auxetic
   polymer component and the appropriate orientation of the main
   crystallographic axes of the single-crystal layers lead to the
   development of large piezoelectric anisotropy and considerable
   hydrostatic piezoelectric response in comparison to the single-crystal
   component. Effective piezoelectric coefficients d(3j){*} and
   electromechanical coupling factors k(3j){*} of the composites obey the
   conditions d(33){*}/d(31){*}vertical bar >= 10, d(33){*}/vertical bar
   d(32){*}vertical bar >= 10,k(33){*}/vertical bar k(31){*}vertical bar >=
   10, and k(33){*}/vertical bar k(32){*} vertical bar >= 10 at k(33){*}
   approximate to 0.6. Hydrostatic piezoelectric coefficients
   d(h){*}approximate to 470 pC/N and g(h){*} similar to (10(2)10(3))mVm/N
   are one to two orders-of-magnitude larger than those of the
   single-crystal component, which make it attractive for various
   piezotechnical applications. The role of the elastic properties of the
   components is discussed in connection with the large hydrostatic
