@article{ WOS:000305966600006,
Author = {Malischewsky, Peter G. and Lorato, Andrea and Scarpa, Fabrizio and
   Ruzzene, Massimo},
Title = {{Unusual behaviour of wave propagation in auxetic structures: P-waves on
   free surface and S-waves in chiral lattices with piezoelectrics}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2012}},
Volume = {{249}},
Number = {{7}},
Pages = {{1339-1346}},
Month = {{JUL}},
Abstract = {{We examine some unusual wave propagation characteristics related to
   auxetic systems represented by continuum isotropic materials and
   hexagonal chiral lattices with and without active piezoelectric
   actuation. We show for the first time a peculiar singularity in the
   ratio between reflected P and S waves in Rayleigh-wave type propagation
   for auxetic isotropic materials, which has been otherwise observed only
   in at least bi-phase material systems. The other unusual phenomenon is a
   strong increase of the pass-stop band frequencies in hexachiral lattices
   with piezoelectric materials, with no change of the shear-wave type
   otherwise occurring in pristine lattice with no piezoelectric
   contribution.}},
