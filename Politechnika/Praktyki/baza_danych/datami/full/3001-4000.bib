@inproceedings{ WOS:000304243100020,
Author = {Rossiter, Jonathan and Scarpa, Fabrizio and Takashima, Kazuto and
   Walters, Peter},
Editor = {{Goulbourne, NC and Ounaies, Z}},
Title = {{Design of a deployable structure with shape memory polymers}},
Booktitle = {{BEHAVIOR AND MECHANICS OF MULTIFUNCTIONAL MATERIALS AND COMPOSITES 2012}},
