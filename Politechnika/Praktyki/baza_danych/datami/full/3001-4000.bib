@article{ WOS:000299985300012,
Author = {Lee, Yongjae and Kao, Chi-Chang and Vogt, Thomas},
Title = {{Thermal Expansion of the Superhydrated Small-Pore Zeolite Natrolite}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
