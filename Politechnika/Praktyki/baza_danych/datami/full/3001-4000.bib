@article{ WOS:000216879500002,
Author = {Erofeev, V. I. and Igumnov, L. A. and Pavlov, I. S.},
Title = {{THE MATHEMATICAL MODEL FOR ANISOTROPIC MATERIAL WITH AUXETIC PROPERTIES}},
Journal = {{MATERIALS PHYSICS AND MECHANICS}},
Year = {{2015}},
Volume = {{23}},
Number = {{1}},
Pages = {{5-9}},
Abstract = {{A two-dimensional model representing a square lattice of round particles
   is proposed for description of auxetic properties of an anisotropic
   crystalline material with cubic symmetry. It is assumed that each
   particle has two translational and one rotational degrees of freedom.
   Differential equations describing the propagation of elastic and
   rotational waves in such a medium have been derived. Relationships
   between the macroelasticity constants of the medium and the parameters
   of its inner structure have been found. It has been shown that the
   Poisson's ratios of the anisotropic material can be negative for certain
   values of the parameters of its inner structure.}},
