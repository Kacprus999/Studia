@article{ WOS:000280222800004,
Author = {Grima, Joseph N. and Gatt, Ruben},
Title = {{Perforated Sheets Exhibiting Negative Poisson's Ratios}},
Journal = {{ADVANCED ENGINEERING MATERIALS}},
Year = {{2010}},
Volume = {{12}},
Number = {{6}},
Pages = {{460-464}},
Month = {{JUN}},
Abstract = {{Sheets made from readily available conventional materials containing
   diamond or star shaped perforations are shown to exhibit various
   Poisson's ratio values which could also be negative (auxetic). This
   behavior may be exhibited in both tension and compression and can be
   explained through models based on ``rotating rigid units.{''} This
   provides an easy manner for the manufacture of auxetic or conventional
   systems at any scale which can be tailor made to exhibit particular
   values of the Poisson's ratio.}},
