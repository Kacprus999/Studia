@article{ WOS:000344482100012,
Author = {Hou, Xiuhui and Deng, Zichen and Yin, Guansheng},
Title = {{Application of transfer matrix method in heat transfer performance
   analysis of multi-re-entrant honeycomb structures}},
Journal = {{HEAT AND MASS TRANSFER}},
Year = {{2014}},
Volume = {{50}},
Number = {{12}},
Pages = {{1765-1782}},
Month = {{DEC}},
Abstract = {{The thermal properties for the multi-re-entrant honeycomb are
   investigated, where the hexagon and re-entrant topologies are applied
   for comparison. A compact model was adopted for the local heat transfer
   rate and pressure drop estimations while the total heat transfer rate
   was analyzed using the transfer matrix method. A thermal performance
   index was specified to characterize a good heat exchange medium that can
   transfer more heat at the expense of lower pressure loss. Numerical
   results reveal better thermal performances of multi-re-entrant
   honeycombs over hexagon and re-entrant topologies, attributed to the
   presence of added base walls. Auxetic effect introduced in
   multi-re-entrant honeycomb generally provides enhanced out-of-plane
   thermal conductivity and increased total heat transfer efficiency due to
   higher surface area density.}},
