@article{ WOS:000282185500012,
Author = {Taniguchi, Yusuke and Ando, Kosei},
Title = {{Time dependence of Poisson's effect in wood II: volume change during
   uniaxial tensile creep}},
Journal = {{JOURNAL OF WOOD SCIENCE}},
Year = {{2010}},
Volume = {{56}},
Number = {{4}},
Pages = {{350-354}},
Abstract = {{To determine the viscoelasticity of wood three-dimensionally, a
   longitudinal tensile creep test was conducted on 12 species of wood to
   examine the change in the rate of volume increase (Delta V/V) with time.
   Immediately after the beginning of creep, Delta V/V was positive, and
   during creep, Delta V/V decreased rapidly, then more gradually. The
   decrease in tangential strain was considered to mainly contribute to the
   decrease in Delta V/V during creep. Immediately after the removal of the
   load, Delta V/V decreased to a negative value; thereafter, it decreased
   slowly and finally reached a certain value. The value of Delta V/V
   during creep tended to decrease with increasing density of wood. Also,
   there was a negative correlation between wood density and the rate of
   increase in Delta V/V.}},
