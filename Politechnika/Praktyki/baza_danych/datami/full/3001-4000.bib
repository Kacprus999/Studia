Author = {Pikhitsa, Peter V. and Choi, Mansoo and Kim, Hyung-Jung and Ahn,
   Sung-Hoon},
Title = {{Auxetic lattice of multipods}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2009}},
Volume = {{246}},
