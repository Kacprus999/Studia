@inproceedings{ WOS:000283747900009,
Author = {Jia, Yan-Bin and Mason, Matthew and Erdmann, Michael},
Editor = {{Chirikjian, GS and Choset, H and Morales, M and Murphey, T}},
Title = {{A State Transition Diagram for Simultaneous Collisions with Application
   in Billiard Shooting}},
Booktitle = {{ALGORITHMIC FOUNDATIONS OF ROBOTICS VIII}},
