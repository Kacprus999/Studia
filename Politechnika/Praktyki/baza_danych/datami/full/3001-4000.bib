@article{ WOS:000316086500018,
Author = {Solis-Mora, I. S. and Palomino-Ovando, M. A. and Perez-Rodriguez, F.},
Title = {{Elastic superlattices with simultaneously negative effective mass
   density and shear modulus}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2013}},
Volume = {{113}},
Number = {{9}},
Month = {{MAR 7}},
Abstract = {{We investigate the vibrational properties of superlattices with layers
   of rubber and polyurethane foam, which can be either conventional or
   auxetic. Phononic dispersion calculations show a second pass band for
   transverse modes inside the lowest band gap of the longitudinal modes.
   In such a band, the superlattices behave as a double-negative elastic
   metamaterial since the effective dynamic mass density and shear modulus
   are both negative. The pass band is associated to a Fabry-Perot
   resonance band which turns out to be very narrow as a consequence of the
