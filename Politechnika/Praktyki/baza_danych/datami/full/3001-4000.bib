@article{ WOS:000272677700004,
Author = {Lue Hexiang and Zheng Yudong and Meng Haoye and Xu Manqiong and Guo
   Xingmin},
Title = {{COMPOSITION AND MICRO-MORPHOLOGY OF POROUS PVA HYDROGELS AND THEIR
   POISSON'S RATIO AND MECHANICAL PROPERTIES}},
Journal = {{ACTA POLYMERICA SINICA}},
Year = {{2009}},
Number = {{11}},
Pages = {{1096-1100}},
Month = {{NOV 20}},
Abstract = {{In order to obtain the auxetic hydrogels with negative Poisson's ratio,
   porous poly(vinyl alcohol) (PVA) hydrogels with different contents of
   PVA and surfactant (alkylphenol ethoxylates : OP) were prepared by
   methods of surfactants template and repeatedly freezing-thawing process,
   according to the microstructure and deformation mechanism of auxetic
   materials. The Poisson's ratio and mechanical properties of porous PVA
   hydrogels were measured and calculated by digital speckle technique. The
   microstructure and pore morphology of porous PVA hydrogels were observed
   by optical and electron microscopy. The interrelation and variation of
   their parameters, including composition, Poisson's ratio, compression
   modulus, volume modulus of pure and porous PVA hydrogels were compared
   and investigated. With the increasing of PVA and OP contents, the pore's
   size became larger, and the Poisson's ratio of porous PVA hydrogels
   decreased. The values of Poisson's ratio of porous PVA hydrogels were
   directly proportional to the ratio of the compression modulus and volume
   modulus of the hydrogels.}},
