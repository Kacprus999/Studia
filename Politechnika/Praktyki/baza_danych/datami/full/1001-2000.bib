@inproceedings{ WOS:000493536900018,
Author = {Borcea, Ciprian S. and Streinu, Ileana},
Editor = {{Kecskemethy, A and Flores, FG and Carrera, E and Elias, DA}},
Title = {{Auxetic Regions in Large Deformations of Periodic Frameworks}},
