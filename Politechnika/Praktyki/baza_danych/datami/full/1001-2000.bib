@inproceedings{ WOS:000473529900035,
Author = {Rysaeva, L. Kh and Klyavlina, A. I. and Galiachmetov, F. Kh and Baimova,
   J. A. and Lisovenko, D. S. and Gorodtsov, V. A.},
Title = {{Deformation behaviour of re-entrant carbon honeycomb structures}},
Booktitle = {{OPEN SCHOOL-CONFERENCE OF NIS COUNTRIES ULTRAFINE GRAINED AND
   NANOSTRUCTURED MATERIALS}},
