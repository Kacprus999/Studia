@article{ WOS:000458877500020,
Author = {Dong, Yulan and Zeng, Bowen and Zhang, Xiaojiao and Li, Dongde and He,
   Jun and Long, Mengqiu},
Title = {{Study on the strain-induced mechanical property modulations in monolayer
   Tellurene}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2019}},
Volume = {{125}},
Number = {{6}},
Month = {{FEB 14}},
Abstract = {{Two-dimensional monolayer Tellurium, termed as Tellurene (Te), has
   recently been fabricated in the experiment. In this work, under uniaxial
   strain applied along either the armchair or zigzag direction with strain
   strength varying from 0 to 40\%, we have investigated the mechanical
   property of Te including three allotropes (alpha-Te, beta-Te, and
   gamma-Te). Our results show that the beta-Te exhibits the most obvious
   anisotropy in the stress-strain curve, Young's modulus, and Poisson's
   ratio, which is thought to originate from its inherent structure
   characteristic. The stress-strain curve demonstrates that the beta-Te
   can withstand relatively high critical strain up to 36\% in the armchair
   direction and 35\% in the zigzag direction. The calculated Young's
   modulus E(theta) of the beta-Te is 55 GPa in the zigzag direction and
   that is 27 GPa in the armchair direction. The comparative high critical
   strain and small Young's modulus mean that beta-Te has good flexibility.
   Moreover, under the strain range within 15\%, the direction being easy
   to be stretched has a reversion, which is thought to be decided by the
   competition between the puckered structure and the transfer charge
   resistant to deformation. Furthermore, when the strain is applied along
   the direction parallel to the pucker of beta-Te, it is interesting to
   find that Poisson's ratio is negative in the out-of-plane direction. The
   hinged structure of the beta-Te contributes to the negative Poisson's
   ratio, which can be comprehended by the hypothetical step-by-step
   evolution process. Published under license by AIP Publishing.}},
