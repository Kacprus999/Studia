@article{ WOS:000453464600003,
Author = {Ipek, S. and Kilic, M. E. and Mogulkoc, A. and Cahangirov, S. and
   Durgun, E.},
Title = {{Semiconducting defect-free polymorph of borophene: Peierls distortion in
   two dimensions}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2018}},
Volume = {{98}},
Number = {{24}},
Month = {{DEC 17}},
Abstract = {{In contrast to the well-defined lattices of various two-dimensional (2D)
   systems, the atomic structure of borophene is sensitive to growth
   conditions and type of the substrate which results in rich polymorphism.
   By employing ab initio methods, we reveal a thermodynamically stable
   borophene polymorph without vacancies which is a semiconductor unlike
   the other known boron sheets, in the form of an asymmetric
   centered-washboard structure. Our results indicate that asymmetric
   distortion is induced due to Peierls instability which transforms a
   symmetric metallic system into a semiconductor. We also show that
   applying uniaxial or biaxial strain gradually lowers the obtained band
   gap and the symmetric configuration is restored following the closure of
   the band gap. Furthermore, while the Poisson's ratio is calculated to be
   high and positive in the semiconducting regime, it switches to negative
   once the metallicity is retrieved. The realization of semiconducting
   borophene polymorphs without defects and tunability of its electronic
   and mechanical response can extend the usage of boron sheets in a
