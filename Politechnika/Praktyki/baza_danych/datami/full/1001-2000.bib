@article{ WOS:000497602400010,
Author = {Hwang, Hsien-Kuei and Chern, Hua-Huai and Duh, Guan-Huei},
Title = {{An asymptotic distribution theory for Eulerian recurrences with
   applications}},
Journal = {{ADVANCES IN APPLIED MATHEMATICS}},
