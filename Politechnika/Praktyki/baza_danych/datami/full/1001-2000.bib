@article{ WOS:000453008100026,
Author = {Wang, Tao and Wang, Liangmo and Ma, Zhengdong and Hulbert, Gregory M.},
Title = {{Elastic analysis of auxetic cellular structure consisting of re-entrant
   hexagonal cells using a strain-based expansion homogenization method}},
Journal = {{MATERIALS \& DESIGN}},
Year = {{2018}},
Volume = {{160}},
Pages = {{284-293}},
Month = {{DEC 15}},
Abstract = {{In this paper, an auxetic cellular structure consisting of re-entrant
   hexagonal cells have been analyzed, exhibiting different elastic
   properties in two orthogonal directions. A novel strain-based expansion
   homogenization method has been proposed and used to contrastively
   analyze the Young's modulus and Poisson's ratio of the re-entrant
   auxetic cellular structure in two principal directions. The comparison
   of the obtained theoretical results with the simulation results and the
   experimental results shows good agreement, which validates the
   effectiveness of the analytical model. With the validated model, the
   dependence of the elastic properties on the geometric parameters of the
   unit cell, including the aspect ratio, length ratio and re-entrant
   angle, has been further investigated. The results also indicate that the
   analyzed auxetic structure features a stiffer modulus along the
   re-entrant direction especially for a small re-entrant angle. Moreover,
   the auxetic structure shows a stronger auxeticity along the re-entrant
   direction when the re-entrant angle is smaller than the transitional
   point. A design map has been presented to show the great potential of
   the structure in function-oriented design, providing an extensive
   reference for the practical engineering application. (C) 2018 Elsevier
   Ltd. This is an open access article under the CC BY-NC-ND license.}},
