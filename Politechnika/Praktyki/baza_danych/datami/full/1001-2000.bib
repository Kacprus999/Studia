@article{ WOS:000415832200026,
Author = {Ren, Xin and Shen, Jianhu and Phuong Tran and Tuan Duc Ngo and Xie, Yi
   Min},
Title = {{Auxetic nail: Design and experimental study}},
Journal = {{COMPOSITE STRUCTURES}},
Year = {{2018}},
Volume = {{184}},
Pages = {{288-298}},
Month = {{JAN 15}},
Abstract = {{Under uniaxial compression (tension), auxetic materials would shrink
   (expand) laterally. It has been speculated that the auxetic property
   could be used to design superior nails for easier push-in and harder
   pull-out. In this study, the first auxetic nails are designed,
   fabricated and experimentally investigated. Pine timber and
   mediumdensity fibreboard are selected as testing materials. The push-in
   and pull-out performance of auxetic and nonauxetic nails is compared by
   using two key parameters of the maximum compressive force and the
   maximum tensile force. It is found that the auxetic nails do not always
   exhibit superior mechanical performance to nonauxetic ones. Also, the
   small auxetic deformation of one typical designed auxetic nail is
   revealed by the experimentally validated finite element model. The
   experimental and numerical results illustrate the limitations of
   exploiting the auxetic property in the nail application. Some
   suggestions are provided for more effective designs of future auxetic
   nails.}},
