@article{ WOS:000443626000002,
Author = {Nguyen Dinh Duc and Pharn Hong Cong},
Title = {{Nonlinear dynamic response and vibration of sandwich composite plates
   with negative Poisson's ratio in auxetic honeycombs}},
Journal = {{JOURNAL OF SANDWICH STRUCTURES \& MATERIALS}},
Year = {{2018}},
Volume = {{20}},
Number = {{6}},
Pages = {{692-717}},
Month = {{SEP}},
Abstract = {{Auxetic cellular solids in the forms of honeycombs under blast load have
   great potential in a diverse range of applications, including core
   material in sandwich plates composite components. Based on Reddy's
   first-order shear deformation plate theory, this paper presents an
   analysis of the nonlinear dynamic response and vibration of sandwich
   plates with negative Poisson's ratio in auxetic honeycombes on elastic
   foundations subjected to blast and mechanical loads. A three-layer
