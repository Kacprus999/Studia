@inproceedings{ WOS:000490712900005,
Author = {Michalski, Jakub and Strek, Tomasz},
Editor = {{Gapinski, B and Szostak, M and Ivanov, V}},
Title = {{Fatigue Life of Auxetic Re-entrant Honeycomb Structure}},
Booktitle = {{ADVANCES IN MANUFACTURING II, VOL 4 - MECHANICAL ENGINEERING}},
Series = {{Lecture Notes in Mechanical Engineering}},
Year = {{2019}},
Pages = {{50-60}},
Note = {{6th International Scientific-Technical Conference on Advances in
   Manufacturing II (MANUFACTURING), Poznan, POLAND, MAY 19-22, 2019}},
Organization = {{Poznan Univ Technol, Fac Mech Engn \& Management; Polish Acad Sci, Comm
   Machine Bldg; Polish Acad Sci, Comm Prod Engn}},
Abstract = {{Auxetic materials have unique properties which still require further
   evaluation. One of the least researched topics in terms of auxetics is
   their fatigue strength. In this article, high-cycle fatigue life of
   auxetic re-entrant structure unit cell was calculated using Finite
   Element Analysis. A fully reversed cycle consisting of alternating
   compressive and tensile loads was applied. Also, stress-life data had to
   be defined as approximated Wohler's curve. Result in form of cycles to
   failure contour plot was compared with corresponding contour plot
   obtained for regular hexagonal honeycomb lattice unit cell. It was
   observed that re-entrant cell has significantly higher fatigue strength
   than non-auxetic honeycomb cell. The test was also carried out for the
   models of the structure made of array of unit cells. Their results
   confirmed the previous observations which bring the conclusion that
   auxetic materials exhibit superior fatigue life properties compared with
   regular microstructures.}},
