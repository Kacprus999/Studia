@article{ WOS:000080479800006,
Author = {Pontius, JS},
Title = {{Estimation of the mean in line intercept sampling}},
Journal = {{ENVIRONMENTAL AND ECOLOGICAL STATISTICS}},
Year = {{1998}},
Volume = {{5}},
Number = {{4}},
Pages = {{371-379}},
Month = {{DEC}},
Abstract = {{Ratio estimation of the parametric mean for a characteristic measured on
   plants sampled by a line intercept method is presented and evaluated via
   simulation using different plant dispersion patterns (Poisson, regular
   cluster, and Poisson cluster), plant width variances, and numbers of
   lines. The results indicate that on average the estimates are close to
   the parametric mean under all three dispersion patterns. Given a fixed
   number of lines, variability of the estimates is similar across
   dispersion patterns with variability under the Poisson pattern slightly
   smaller than variability under the cluster patterns. No variance
   estimates were negative under the Poisson pattern, but some estimates
