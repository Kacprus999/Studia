@article{ WOS:A1992KB89600032,
Author = {HOFFMAN, LL},
Title = {{CHARACTERIZING LINEAR BIRTH AND DEATH PROCESSES}},
Journal = {{JOURNAL OF THE AMERICAN STATISTICAL ASSOCIATION}},
