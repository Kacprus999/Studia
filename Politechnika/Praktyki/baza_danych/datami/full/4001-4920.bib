@article{ WOS:000071379900002,
Author = {Loureiro, MA and Lakes, RS},
Title = {{Scale-up of transformation of negative Poisson's ratio foam: Slabs}},
Journal = {{CELLULAR POLYMERS}},
Year = {{1997}},
Volume = {{16}},
Number = {{5}},
Pages = {{349-363}},
Abstract = {{This paper presents an experimental study performed on the scale-up of
   processing of negative Poisson's ratio foam. These materials become
   thinner under compression and fatter under tension, opposite to ordinary
   materials. The transformation process consists of triaxial compression
   combined with heat treatment. Transformed foam cells have a concave or
   ``re-entrant{''} shape. Scale-up is achieved by use of a modified mould
   assembled around the foam slab and longer processing times. Foam with a
