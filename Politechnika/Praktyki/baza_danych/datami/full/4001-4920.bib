Title = {{Vibro-acoustics and damping analysis of negative Poisson's ratio
   honeycombs}},
Booktitle = {{PASSIVE DAMPING AND ISOLATION - SMART STRUCTURES AND MATERIALS 1998}},
Series = {{PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)}},
