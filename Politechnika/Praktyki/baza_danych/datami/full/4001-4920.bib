@inproceedings{ WOS:000238445800006,
Author = {Hanson, D. and Bergs, R. and Tadesse, Y. B. and White, V. and Priya, S.},
Editor = {{BarCohen, Y}},
Title = {{Enhancement of EAP actuated facial expressions by designed chamber
   geometry in elastomers}},
Booktitle = {{SMART STRUCTURES AND MATERIALS 2006: ELECTROACTIVE POLYMER ACTUATORS AND
