Author = {Alderson, A and Davies, PJ and Evans, KE and Alderson, KL and Grima, JN},
Title = {{Modelling of the mechanical and mass transport properties of auxetic
   molecular sieves: an idealised inorganic (zeolitic) host-guest system}},
Journal = {{MOLECULAR SIMULATION}},
