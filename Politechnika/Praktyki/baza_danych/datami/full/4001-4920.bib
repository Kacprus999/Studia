@article{ WOS:000175824300001,
Author = {Whitty, JPM and Nazare, F and Alderson, A},
Title = {{Modelling the effects of density variations on the in-plane Poisson's
   ratios and Young's moduli of periodic conventional and re-entrant
   honeycombs - Part 1: Rib thickness variations}},
Journal = {{CELLULAR POLYMERS}},
Year = {{2002}},
Volume = {{21}},
Number = {{2}},
Pages = {{69-98}},
Abstract = {{Finite Element (FE) modelling is used to investigate the effect of
   varying the rib thickness on the mechanical properties of conventional
   and re-entrant periodic honeycombs. The calculations confirm that the
   re-entrant honeycomb has a negative Poisson's ratio. The data show that
   the mechanical properties vary not only with honeycomb relative density,
   but also on how the relative density variation is achieved. Reducing the
   thickness of `vertical' ribs, whilst maintaining constant `diagonal' rib
   thickness, does not alter the Young's modulus and Poisson's ratio in the
   x direction (E-x and V-xv), but does lead to a decrease in the magnitude
   of the y-directed properties (E-v and V-vx). Reducing the diagonal rib
   thickness at constant vertical rib thickness leads to a more marked
   decrease in E-v as well as in E-x, whereas V-xv and V-vx both increase
   in magnitude. An analytical model is also developed to account for
   different vertical and diagonal rib thicknesses, and good agreement with
   the FE model data is achieved. The results have implications for
   applications in which maximising or maintaining mechanical properties
   with respect to relative density is important.}},
