@article{ WOS:A1993MQ16400024,
Author = {BOAL, DH and SEIFERT, U and SHILLCOCK, JC},
Title = {{NEGATIVE POISSON RATIO IN 2-DIMENSIONAL NETWORKS UNDER TENSION}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{1993}},
Volume = {{48}},
Number = {{6}},
Pages = {{4274-4283}},
Month = {{DEC}},
Abstract = {{The elastic properties of two-dimensional networks under tension are
   studied by the mean-field appoximation and Monte Carlo simulation. The
   networks are characterized by fixed (polymerized) connectivity and
   either a square-well or a Hooke's?aw interaction among their components.
   Both self-avoiding and phantom networks are examined. The elastic
   properties of Hooke's-law networks at large equilibrium length are found
   to be well represented by a mean-field model. All the networks
   investigated show a negative Poisson ratio over a range of tension. At
   finite tension, the phantom networks exhibit a phase transition to a
