@article{ WOS:000175276500006,
Author = {Flavin, J and Gleeson, B},
Title = {{Pointwise and other decay estimates for an isotropic elastic strip}},
Journal = {{JOURNAL OF ELASTICITY}},
Year = {{2001}},
Volume = {{64}},
Number = {{2-3}},
Pages = {{191-197}},
Abstract = {{This paper is concerned with an homogeneous isotropic linear elastic
   strip, in plane strain. It is supposed that its lateral boundaries are
   displacement-free and that the deformation is generated by actions on
   the ends. A cross-sectional measure of deformation, complementing that
   of a previous paper, is defined and shown to satisfy a generalised
   convexity condition in the axial variable x(1), for materials with
   negative Poisson's ratio sigma. An enhanced measure is subsequently
   defined, and, in the case of a semi-infinite strip, is shown to yield
   pointwise exponential decay estimates for both the axial and the
   transverse displacement components for materials with positive Poisson's
   ratio.}},
Publisher = {{KLUWER ACADEMIC PUBL}},
Address = {{VAN GODEWIJCKSTRAAT 30, 3311 GZ DORDRECHT, NETHERLANDS}},
