@article{ WOS:000230536900001,
Author = {Belec, L and Blankenship, JW and Lubell, WD},
Title = {{Examination of structural characteristics of the potent oxytocin
   antagonists (dPen(1) Pen(6))-OT and (dPen(1) Pen(6), 5-(t)BuPro(7))-OT
   by NMR, raman, CD spectroscopy and molecular modeling}},
Journal = {{JOURNAL OF PEPTIDE SCIENCE}},
Year = {{2005}},
Volume = {{11}},
Number = {{7}},
Pages = {{365-378}},
Month = {{JUL}},
Abstract = {{The synthesis and biological evaluation of
   perricillamine(6)-5-tert-butylproline(7)-oxytocin analogs and comparison
   with their proline(7)-oxytocin counterparts has led to the discovery of
   two potent oxytocin (OT) antagonists: {[}dPen(1),Pen(6)]-oxytocin (1,
   pA(2) = 8.22, EC50 - 6.0 nm) and {[}dPen(1),Pen(6),5-tBuPro(7)]-oxytocin
   (2, pA(2) = 8.19, EC50 = 6.5 nM). In an attempt to understand the
   conformational requirements for their biological activity, spectroscopic
   analyses of 1 and 2 were perfonned using H-1 NMR, laser Raman and CD
   techniques. In H2O, oxytocin analogs 1 and 2 exhibited cis-isomer
   populations of 7\% and 35\%, respectively. Measurement of the amide
   proton temperature coefficients revealed solvent shielded hydrogens for
   Gin(4) and Pen6 in the major trans-conformer of 1 as well as for Gln(4)
   in the minor cis-conformer of 2. Few long-distance NOES were observed,
   suggesting conformational averaging for analogs 1 and 2 in water;
   moreover, a lower barrier (16.6 +/- 0.2 kcal/mol) for isomerization of
   the amide N-terminal to 5-tBuPro(7) relative to OT was calculated from
   measuring the coalescence temperature of the GlY(9) backbone Nit signals
   in the NMR spectra of 2. Observed bands in the Raman spectra of 1 and 2
   correspond to C-beta-S-S-C-beta dihedral angles of +110-115 degrees and
   90 degrees, respectively. In water, acetonitrile and methanol, the CD
   spectra for 1 xhibited a positive maximum around 236-239 nm; in
   trifluoroethanol, the spectra shifted and a negative maximum was
   observed at 240 nm. The CID spectra of 2 were unaffected by solvent
