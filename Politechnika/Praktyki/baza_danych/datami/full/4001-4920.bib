@article{ WOS:000231370000003,
Author = {Smith, CW and Tan, KH and Evans, KE and Resendes, R and Bartole-Scott, A
   and Manners, I},
Title = {{Mechanical properties of the novel organometallic polymer
   poly(ferrocenyldimethylsilane)}},
Journal = {{JOURNAL OF POLYMER SCIENCE PART B-POLYMER PHYSICS}},
Year = {{2005}},
Volume = {{43}},
Number = {{17}},
Pages = {{2280-2288}},
Month = {{SEP 1}},
Abstract = {{A study of the mechanical properties of poly(ferrocenyldimethylsilane)
   {[}Fe(eta-C5H4)(2)SiMe2](n), 3, a novel organometallic polymer, has been
   performed on thin films of this material. The Young's modulus and
   Poisson's ratio of film samples (15 x I x 1 mm) of 3 were measured in
   quasi-static tension using a video extensometer. For 3, the values of
   the Young's moduli (E) and Poisson's ratios W were similar between axes
   in the plane and independent of the splicing direction used during
   sample preparation. The mean and standard deviation of the Young's
   modulus and Poisson's ratio were 0.78 +/- 0.08 GPa and 0.37 +/- 0.06
   GPa, respectively. Thermomechanical analysis of 3 revealed a steady
   decrease of E from a room temperature value of approximately 0.70 GPa.
   Additionally, it was found that at 150 degrees C, 3 was unable to
   support even small stresses, consistent with the onset of a melt
   transition (ca. 135 degrees C). A mathematical model based on molecular
   geometry is developed to describe the results. (c) 2005 Wiley
   Periodicals, Inc.}},
