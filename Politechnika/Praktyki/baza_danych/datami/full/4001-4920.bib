@inproceedings{ WOS:000222604600027,
Author = {Dmitriev, SV and Yoshikawa, N and Vasiliev, AA},
Editor = {{Kitagawa, H and Shibutani, Y}},
Title = {{Topological soliton dynamics in media with microscopic rotations}},
Booktitle = {{IUTAM SYMPOSIUM ON MESOSCOPIC DYNAMICS OF FRACTURE PROCESS AND MATERIALS
   STRENGTH}},
Series = {{Solid Mechanics and its Applications}},
Year = {{2004}},
Volume = {{115}},
