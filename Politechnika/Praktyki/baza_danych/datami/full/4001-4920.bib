Title = {{Poisson's Ratios of Technological Materials}},
Booktitle = {{2009 JOINT MEETING OF THE EUROPEAN FREQUENCY AND TIME FORUM AND THE IEEE
   INTERNATIONAL FREQUENCY CONTROL SYMPOSIUM, VOLS 1 AND 2}},
Series = {{IEEE International Frequency Control Symposium}},
