@article{ WOS:000601316500001,
Author = {Zhang, Qicheng and Lu, Wenjiang and Scarpa, Fabrizio and Barton, David
   and Lakes, Roderic S. and Zhu, Yunpeng and Lang, Ziqiang and Peng,
   Hua-Xin},
Title = {{Large stiffness thermoformed open cell foams with auxeticity}},
Journal = {{APPLIED MATERIALS TODAY}},
Year = {{2020}},
Volume = {{20}},
Month = {{SEP}},
Abstract = {{Open cell foams with auxeticity and modulus 5 times larger than the
   stiffest auxetic polymeric open cell foam featured in scientific
   literature are developed in this paper by using vacuum bags and
   autoclave following a thermoforming process. The resulting foam is
   transverse isotropic with microstructures exhibiting elongated
   semi-reticulated configurations, and contains a plane in which the
   auxetic behavior is evident. Tensile and compression quasi-static tests
   were carried out on samples cut along different directions and with
   different thicknesses. Foams have been also subjected to tensile
   training at different maximum strains to assess their mechanical
   performance. The tensile modulus and Poisson's ratio values of the
   auxetic foams produced in this work can reach 25 MPa and-0.4 in one
   plane and can feature a negative Poisson's ratio up to-1.3 for thinner
   specimens. Smaller thicknesses and higher tensile training strains can
   also reduce the stiffness but enhance the auxeticity of this porous
   material. (c) 2020 Elsevier Ltd. All rights reserved.}},
Publisher = {{ELSEVIER}},
Address = {{RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Zhang, QC (Corresponding Author), Univ Bristol, Bristol Composites Inst ACCIS, Bristol BS8 1TR, Avon, England.
