@article{ WOS:000606869000005,
Author = {Heogh, Woongbeom and Park, Joon B. and Park, Gyung-Jin},
Title = {{Structural Optimization of 2D Auxetic Structures Having User-defined
   Poisson's Ratio}},
Journal = {{TRANSACTIONS OF THE KOREAN SOCIETY OF MECHANICAL ENGINEERS A}},
Year = {{2020}},
Volume = {{44}},
Number = {{7}},
Pages = {{495-505}},
Month = {{JUL}},
Abstract = {{Poisson's ratio is the ratio of the displacement in the lateral
   direction to the displacement in the longitudinal direction when a
   material is deformed. The value of Poisson's ratio is constant for
   isotropic and homogeneous materials. The range of Poisson's ratio varies
   from -1 to 0.5 theoretically, but the value is typically positive.
   Recently, negative Poisson's ratios have attracted significant attention
   to researchers. Negative values are observed in some artificial
   structures; however, these structures do not exhibit isotropic
   characteristics. In this study, the characteristics of negative
   Poisson's ratios were investigated, and it was found that reentrant
   lattice structures can have negative Poisson's ratio values. Conceptual
   design for such structures was carried out, and detailed design
   considering the isotropic characteristics was performed through
   optimization to extremize the negative value of the Poisson's ratio. The
   optimization formulation was defined, and the size/shape structural
   optimization techniques were employed in finalizing the structures.}},
