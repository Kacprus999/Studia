@article{ WOS:000620261000012,
Author = {Gao, Yifan and Wen, Minru and Wang, Shaoxiong and Yu, Hao and Zhang, Xin
   and Wu, Fugen and Dong, Huafeng},
Title = {{Two-dimensional arsenene polymorph beyond the auxetic foam: high
   mechanical sensitivity and large, negative NPR}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
Year = {{2021}},
Volume = {{23}},
Number = {{6}},
Pages = {{3837-3843}},
Month = {{FEB 14}},
Abstract = {{Single-layer delta-As and gamma-P have unique atomic arrangement, which
   belong to the Pmc2(1) and Pbcm space groups, respectively. Because of
   the coupling hinge structure, the physical properties of the two
   materials have obvious anisotropy. In this paper, we report the
   mechanical properties of the single-layer delta-As and gamma-P. That is,
   their inherent negative Poisson ratio (NPR) is -0.708 and -0.226,
   respectively. Surprisingly, the absolute value of the NPR of delta-As is
   approximately 26.2 times greater than that of single-layer black
   phosphorus (the NPR of single-layer black phosphorus is -0.027), and
   remains invariant at a certain strain range. Thus, single-layer delta-As
   will have huge potential applications in nanosensors and electronic
   wearable devices due to its invariant and large, negative NPR.}},
