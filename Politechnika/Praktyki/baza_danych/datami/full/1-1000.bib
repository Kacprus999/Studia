@article{ WOS:000635715500174,
Author = {Rawte, Prajit S. and Upadhyaya, Priyank},
Title = {{Numerical modelling of bilayer tetra periodic metamaterials to predict
   the elastic response}},
Journal = {{MATERIALS TODAY-PROCEEDINGS}},
