@article{ WOS:000519576300011,
Author = {Singh, Sandeep},
Title = {{Critical assessment of the interatomic potentials for the elastic
   properties of the noncarbon monolayer nanomaterials}},
Journal = {{COMPUTATIONAL MATERIALS SCIENCE}},
Year = {{2020}},
Volume = {{177}},
Month = {{MAY}},
Abstract = {{The present work deals with the assessment of Tersoff potential with
   different sets of empirical parameters for the two-dimensional noncarbon
   beta N (beta: B, Al, Ga and In) hexagonal monolayer lattice structures
   in the framework of Cauchy-Born rule. The deformations at atomistic and
   continuum scale are coupled thorough Cauchy-Born rule accounting for
   curvature effects. The interatomic potentials are assessed to
   demonstrate their accuracy for predicting the elastic properties,
   bending rigidity, cohesive energy and bond length when compared with
   those predicted from experimental studies and first-principle
   calculations. The elastic properties and bending rigidity predicted with
   different sets of empirical parameters for interatomic potentials are
   found to be scattered in a significant manner and best set of parameters
   is identified here and recommended for further use in multiscale
   simulations. Few of the parameter sets for the Tersoff potential
   predicted a negative value of the Poisson's ratio through multiscale
   simulation. Furthermore, the bending rigidity of the rest of the three
   nanomaterials, except boron nitride nanomaterial, is reported for the
   first time in the present study.}},
