@article{ WOS:000672028500001,
Author = {He, Linxin and Li, Xinxin and Zhu, Xin and Luo, Jianglei and Chen,
   Zhiqian and Li, Chunmei},
Title = {{New graphane: inspiration from the structure correlation with
   phosphorene}},
Journal = {{PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
Year = {{2021}},
Volume = {{23}},
Number = {{28}},
Pages = {{15302-15312}},
Month = {{JUL 28}},
Abstract = {{The application of phosphorene and graphane in different photoelectric
   devices and energy reserves has attracted wide attention. Here, we
   investigated the Raman spectra, phonon dispersion and vibration modes of
   four phosphorene monolayer polymorphs and four graphane allotropes with
   the corresponding crystal structures to analyze the structure
   correlation between them. Based on the ``three identical, one
   divergent{''} pattern found in the sp(3) hybrid atomic orbitals of
   phosphorene and graphane, four new graphane conformers with different
   hydrogenation modes named gamma delta-G, alpha gamma-G, beta gamma-G and
   alpha delta-G are successfully predicted. Among these four new graphane
   conformers, beta gamma-G has the lowest binding energy, which is only
   0.02 eV per atom higher than beta-G, the most stable one among all
   graphane theoretically predicted. This means that beta gamma-G may
   co-exist with beta-G during the experimental synthesis of graphane,
   which can be distinguished from the side views with threefold structures
   for beta gamma-G and twofold structures for beta-G. All the new graphane
   conformers are direct-band-gap semiconductors with band gaps more than 3
   eV, which indicate their great potential in optoelectronic devices.
   Furthermore, three of them exhibit in-plane negative Poisson's ratios
   under tensile deformation.}},
