@article{ WOS:000517075400001,
Author = {Xu, Wanli and Ma, Pibo and Wu, Liwei and Jiang, Gaoming},
Title = {{Low-velocity impact properties of composite reinforced by auxetic
   warp-knitted spacer fabric}},
Journal = {{JOURNAL OF SANDWICH STRUCTURES \& MATERIALS}},
Abstract = {{Auxetic warp-knitted spacer fabric composite is famous for its excellent
   mechanical properties and has been applied in many potential fields,
   such as automobile industry, aerospace field, and so on. Four kinds of
   composites with different negative Poisson's ratios were used for
   low-velocity impact tests. The influence of different negative Poisson's
   ratio effect and initial impact energy on the impact resistance and
   energy absorption ability of composites are studied. Result shows that
   the peak impact load, energy absorption, and impact resistance of
   composites increase with an increase in the auxetic effect of
   reinforcement. The composite with better auxetic effect shows lower
   damage level after low-velocity impact. And the higher initial impact
   energy can easily weaken the impact resistance ability of composites.}},
