@article{ WOS:000407919400001,
Author = {Dudek, K. K. and Wolak, W. and Dudek, M. R. and Caruana-Gauci, R. and
   Gatt, R. and Wojciechowski, K. W. and Grima, J. N.},
Title = {{Programmable magnetic domain evolution in magnetic auxetic systems}},
Journal = {{PHYSICA STATUS SOLIDI-RAPID RESEARCH LETTERS}},
Year = {{2017}},
Volume = {{11}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{In this work, we investigate the evolution of magnetic domains with time
   in magnetic auxetic systems using the Ising model. We show that the
   mechanical deformation of the structure affects the way how domains
   evolve and that their rate of growth does not follow a well-known power
   law associated with the Ising model for systems corresponding to a
   specific distance between Ising spins. We also show that the variation
   in the rate of deformation of the system leads to completely different
   results in terms of the change in size of magnetic domains and domain
   energy. This means that upon changing the speed used to deform the
   system, it is possible to control the growth of magnetic domains and
   hence control the magnetic properties of the whole system. This may
   prove to be very important in the case of potential applications such as
   magnetic freezers where the magnetocaloric effect resulting with a
   change in temperature could be affected by the rate of deformation of
   the discussed auxetic structure.}},
