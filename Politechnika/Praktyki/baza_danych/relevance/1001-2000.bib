@inproceedings{ WOS:000446349800002,
Author = {Solheim, Hannah and Stanisauskis, Eugenia and Miles, Paul and Oates,
   William},
Editor = {{Naguib, HE}},
Title = {{Fractional Viscoelasticity of Soft Elastomers and Auxetic Foams}},
Booktitle = {{BEHAVIOR AND MECHANICS OF MULTIFUNCTIONAL MATERIALS AND COMPOSITES XII}},
Series = {{Proceedings of SPIE}},
Year = {{2018}},
Volume = {{10596}},
Note = {{Conference on Behavior and Mechanics of Multifunctional Materials and
   Composites XII, Denver, CO, MAR 05-08, 2018}},
Organization = {{SPIE; OZ Opt Ltd; Polytec Inc}},
Abstract = {{Dielectric elastomers are commonly implemented in adaptive structures
   due to their unique capabilities for real time control of a structure's
   shape, stiffness, and damping. These active polymers are often used in
   applications where actuator control or dynamic tunability are important,
   making an accurate understanding of the viscoelastic behavior critical.
   This challenge is complicated as these elastomers often operate over a
   broad range of deformation rates. Whereas research has demonstrated
   success in applying a nonlinear viscoelastic constitutive model to
   characterize the behavior of Very High Bond (VHB) 4910, robust
   predictions of the viscoelastic response over the entire range of time
   scales is still a significant challenge. An alternative formulation for
   viscoelastic modeling using fractional order calculus has shown
   significant improvement in predictive capabilities. While fractional
   calculus has been explored theoretically in the field of linear
   viscoelasticity, limited experimental validation and statistical
   evaluation of the underlying phenomena have been considered. In the
   present study, predictions across several orders of magnitude in
   deformation rates are validated against data using a single set of model
   parameters. Moreover, we illustrate the fractional order is material
   dependent by running complementary experiments and parameter estimation
   on the elastomer VHB 4949 as well as an auxetic foam. All results are
   statistically validated using Bayesian uncertainty methods to obtain
   posterior densities for the fractional order as well as the hyperelastic
   parameters.}},
