@article{ WOS:000259690800090,
Author = {Coluci, V. R. and Hall, L. J. and Kozlov, M. E. and Zhang, M. and
   Dantas, S. O. and Galvao, D. S. and Baughman, R. H.},
Title = {{Modeling the auxetic transition for carbon nanotube sheets}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2008}},
Volume = {{78}},
Number = {{11}},
Month = {{SEP}},
Abstract = {{A simple model is developed to predict the complex mechanical properties
   of carbon nanotube sheets (buckypaper) {[}L. J. Hall et al., Science
   320, 504 (2008)]. Fabricated using a similar method to that deployed for
   making writing paper, these buckypapers can have in-plane Poisson's
   ratios changed from positive to negative, becoming auxetic, as
   multiwalled carbon nanotubes are increasingly mixed with single-walled
   carbon nanotubes. Essential structural features of the buckypapers are
   incorporated into the model: isotropic in-plane mechanical properties,
   nanotubes preferentially oriented in the sheet plane, and freedom to
   undergo stress-induced elongation by both angle and length changes. The
   expressions derived for the Poisson's ratios enabled quantitative
   prediction of both observed properties and remarkable new properties
   obtainable by structural modification.}},
