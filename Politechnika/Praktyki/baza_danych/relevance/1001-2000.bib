@article{ WOS:000360828600010,
Author = {Feng, Changyong and Wang, Hongyue and Han, Yu and Xia, Yinglin and Lu,
   Naiji and Tu, Xin M.},
Title = {{Some Theoretical Comparisons of Negative Binomial and Zero-Inflated
