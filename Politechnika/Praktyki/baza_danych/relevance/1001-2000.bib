@article{ WOS:000655027500093,
Author = {Wang, Zhenwei and Luan, Congcong and Liao, Guangxin and Liu, Jiapeng and
   Yao, Xinhua and Fu, Jianzhong},
Title = {{High-Performance Auxetic Bilayer Conductive Mesh-Based Multi-Material
   Integrated Stretchable Strain Sensors}},
Journal = {{ACS APPLIED MATERIALS \& INTERFACES}},
Year = {{2021}},
Volume = {{13}},
Number = {{19}},
Pages = {{23038-23048}},
Month = {{MAY 19}},
Abstract = {{High-performance stretchable strain sensors, particularly those with
   high sensitivity and broad sensing range, are highly important for
   wearable devices. Herein, a novel auxetic bilayer conductive mesh strain
   sensor (ABSS), composed of multi-hardness silicones, is proposed and
   fabricated by the direct ink writing 3D printing and ink spraying
   technique. The bilayer conductive mesh comprises a thin layer of
   high-conductive and crack-prone single-walled carbon nanotubes (SWCNTs)
   coated on a stretchable carbon-black-doped Ecoflex silicone rubber
   (CB/Ecoflex) mesh. The former serves as the dominant sensing material by
   generating SWCNT cracks in the full strain range, while the latter
   mainly plays the roles of both generating the resistance change and
   maintaining the conductive paths under high strain conditions. The
   presence of high-hardness auxetic frame contributes to the formation of
   longitudinal SWCNT cracks on transverse meshes, enhancing the
   sensitivity of the sensors. It is shown that the synergistic effect of
   the bilayer conductive mesh, strain concentration, and auxetic
   deformation strategy endow ABSS with a high gauge factor (similar to
   13.4) that is 6.6 times larger than that of the common sensor.
   Additionally, this study demonstrates the superior sensing performance
   of the ABSS for wearable applications including swallowing recognition,
   respiration monitoring, and joint movement detection.}},
