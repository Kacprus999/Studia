@article{ WOS:000516807700001,
Author = {Zhu, Shaowei and Tan, Xiaojun and Chen, Shuai and Wang, Bing and Ma, Li
   and Wu, Linzhi},
Title = {{Quasi-All-Directional Negative Stiffness Metamaterials Based on Negative
