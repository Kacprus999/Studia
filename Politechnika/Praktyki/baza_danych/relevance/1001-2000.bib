@article{ WOS:000263733200018,
Author = {Pour, Nir and Altus, Eli and Basch, Harold and Hoz, Shmaryahu},
Title = {{The Origin of the Auxetic Effect in Prismanes: Bowtie Structure and the
   Mechanical Properties of Biprismanes.}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2009}},
Volume = {{113}},
Number = {{9}},
Pages = {{3467-3470}},
Month = {{MAR 5}},
Abstract = {{Prismanes have been shown to exhibit negative Poisson's ratio (auxetic
   behavior) for their internal rings and normal Poisson's behavior for the
   terminal rings. Various {[}3], {[}4], {[}5], and {[}6] biprismanes were
   studied by using quantum mechanical ab initio calculations. The unusual
   mechanical behavior of prismanes upon stretching was traced to the
   relaxation of geometrical distortion existing in the ground state. The
   increase of the Poissonic effect on the ring size correlates well with
   the Thorpe-Ingold effect. In biprismanes where the CH units in the
   external rings are replaced by nitrogen, the effect is accentuated.
   Substitution of the hydrogens by fluorine atoms markedly reduces the
   effect, this being an interesting example of substituent effect on
   mechanical properties. This effect is attenuated in longer prismanes.}},
