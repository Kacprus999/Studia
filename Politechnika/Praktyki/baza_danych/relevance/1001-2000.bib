@article{ WOS:000604967500021,
Author = {Jia, Li and Wang, Xiaona and Yan, Shinong and Li, Zhiqiang and Zhang,
   Yongmei},
Title = {{Elastic anisotropies and thermodynamic properties of metal dodecborides
   under high pressure}},
Journal = {{JOURNAL OF CHEMICAL THERMODYNAMICS}},
Year = {{2021}},
Volume = {{154}},
Month = {{MAR}},
Abstract = {{The elastic anisotropy and thermodynamic properties of MB12 (M = Hf, Y,
   Zr and Sc) compounds have been investigated by combining the first
   principles with the quasi-harmonic Debye model. The negative values of
   cohesive energy indicate these compounds are thermodynamic stability.
   The results of elastic constants and elastic modulus explain that these
   compounds exhibit high hardness and strong antiuniaxial stress ability.
   But the Vickers hardness values are all lower than that of the
   traditional threshold for superhard material (40 GPa). The BIG ratio and
   Poisson's ratio v reveal that all MB12 compounds are brittle at 0 GPa.
   ZrB12 and HfB12 possess similar fracture toughness, while ScB12
   possesses the lowest one. The 3D contours of the Young's modulus of four
   materials are very similar at both 0 GPa and 100 GPa. Also, pressure can
   increase the anisotropy for MB12 compounds. The pressure and temperature
   dependence of the heat capacity, thermal expansion coefficient and Debye
   temperature of MB12 compounds are also comprehensively discussed. These
   findings are of great value for the applications of these
   multifunctional materials in electromechanical and medical engineering.
   (C) 2020 Elsevier Ltd.}},
