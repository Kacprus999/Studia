@article{ WOS:000373935400010,
Author = {Preisser, John S. and Das, Kalyan and Long, D. Leann and Divaris, Kimon},
Title = {{Marginalized zero-inflated negative binomial regression with application
   to dental caries}},
Journal = {{STATISTICS IN MEDICINE}},
