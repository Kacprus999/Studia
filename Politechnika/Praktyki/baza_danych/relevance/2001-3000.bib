@article{ WOS:000235310400014,
Author = {Jindal, P and Jindal, VK},
Title = {{Strains in axial and lateral directions in carbon nanotubes}},
Journal = {{JOURNAL OF COMPUTATIONAL AND THEORETICAL NANOSCIENCE}},
Year = {{2006}},
Volume = {{3}},
Number = {{1}},
Pages = {{148-152}},
Month = {{FEB}},
Abstract = {{Analytic expressions for strains in carbon nanotubes of all chiralities
   in lateral and axial directions have been obtained. These expressions
   have been related to bond angle and bond length flexibilities. This
   finally results in obtaining very generalized results for Poisson ratio
   of all chirality and P: all diameter carbon nanotubes. It turns out that
   the Poisson ratio is always independent of carbon nanotube diameter, but
   depends on their chirality. On the assumption of fixed bond lengths,
   (only flexible bond angle), the Poisson ratio of all chirality tubes is
   obtained to be 1. When realistic flexibility of bond lengths and bond
   angles is assumed, the Poisson ratio is between similar to 0 and 0.3,
   depending upon chirality. There is thus a compensation in lateral
   dimension when both, bond lengths and bond angles are assumed flexible,
   which has also been described as possibly a cause of very small or
   negative thermal expansion of single nanotubes. Further, it has been
   found that the Poisson ratio is sensitive to applied initial stress,
   varying significantly when obtained under different initial stress
   values. It is observed that Poisson ratio can be used to justify the
   interaction potential by finding out the bond length flexibility in
   comparison to bond angle flexibility.}},
