@inproceedings{ WOS:000610424800017,
Author = {Chin, Lillian and Lipton, Jeffrey and MacCurdy, Robert and Romanishin,
   John and Sharma, Chetan and Rus, Daniela},
Book-Group-Author = {{IEEE}},
Title = {{Compliant Electric Actuators Based on Handed Shearing Auxetics}},
Booktitle = {{2018 IEEE INTERNATIONAL CONFERENCE ON SOFT ROBOTICS (ROBOSOFT)}},
Year = {{2018}},
Pages = {{100-107}},
Note = {{1st IEEE-RAS International Conference on Soft Robotics (RoboSoft),
   Livorno, ITALY, APR 24-28, 2018}},
Organization = {{IEEE; RAS}},
Abstract = {{In this paper, we explore a new class of electric motor-driven compliant
   actuators based on handed shearing auxetic cylinders. This technique
   combines the benefits of compliant bodies from soft robotic actuators
   with the simplicity of direct coupling to electric motors. We
   demonstrate the effectiveness of this technique by creating linear
   actuators, a four degree-of-freedom robotic platform, and a soft robotic
   gripper. We compare the soft robotic gripper against a state of the art
   pneumatic soft gripper, finding similar grasping performance in a
   significantly smaller and more energy-efficient package.}},
