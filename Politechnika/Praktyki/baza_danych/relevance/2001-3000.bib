Address = {{2000 FLORIDA AVE NW, WASHINGTON, DC 20009}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{HYNES, A (Corresponding Author), MCGILL UNIV,DEPT GEOL SCI,MONTREAL H3A 2A7,QUEBEC,CANADA.}},
DOI = {{10.1029/91TC00643}},
