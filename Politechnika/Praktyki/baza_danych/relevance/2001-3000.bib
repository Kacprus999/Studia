@article{ WOS:000413416900007,
Author = {Raranskyi, M. D. and Oliynich-Lysyuk, A. V. and Taschuk, O. Yu.},
Title = {{FEATURES OF MICROPLASTIC STRAIN OF AUXETIC BERYLLIUM IRRADIATED WITH
   HIGH-ENERGY ELECTRONS}},
Journal = {{PROBLEMS OF ATOMIC SCIENCE AND TECHNOLOGY}},
Year = {{2016}},
Number = {{2}},
Pages = {{46-52}},
Abstract = {{In this paper, the method of low-frequency internal friction (1...3 Hz),
   the investigation of behaviour of the dynamic modulus of torsion (G(ef))
   and mathematical simulation of dislocation motion are used to study the
   microplastic strain in naturally aged auxetic beryllium irradiated with
   high-energy (18.eV) electrons. It is discovered that with increase in
   radiation dose, the internal friction and the dislocation velocity
   increase by a factor of 2-3. The stage character of microplastic strain
   of auxetic.. is established. With the aid of mathematical simulation it
   is demonstrated that strain in the irradiated material is due to
   accelerated motion of twinning dislocations at the initial stage which
   is changed to abnormal dynamic deceleration of full dislocations with
   increasing degree of strain at the second stage. It is shown that
   theoretically estimated values are in good agreement with the
   experimentally determined ones.}},
