@article{ WOS:000305913100001,
Author = {Famoye, Felix},
Title = {{Comparisons of some bivariate regression models}},
Journal = {{JOURNAL OF STATISTICAL COMPUTATION AND SIMULATION}},
Year = {{2012}},
Volume = {{82}},
Number = {{7}},
Pages = {{937-949}},
Abstract = {{The bivariate negative binomial regression (BNBR) and the bivariate
   Poisson log-normal regression (BPLR) models have been used to describe
   count data that are over-dispersed. In this paper, a new bivariate
   generalized Poisson regression (BGPR) model is defined. An advantage of
   the new regression model over the BNBR and BPLR models is that the BGPR
   can be used to model bivariate count data with either over-dispersion or
   under-dispersion. In this paper, we carry out a simulation study to
   compare the three regression models when the true data-generating
   process exhibits over-dispersion. In the simulation experiment, we
