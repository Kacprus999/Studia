@article{ WOS:000323821700002,
Author = {Buonaguidi, Bruno and Muliere, Pietro},
Title = {{On the Wald's Sequential Probability Ratio Test for Levy Processes}},
Journal = {{SEQUENTIAL ANALYSIS-DESIGN METHODS AND APPLICATIONS}},
Year = {{2013}},
Volume = {{32}},
Number = {{3}},
Pages = {{267-287}},
Abstract = {{The Wald's sequential probability ratio test (SPRT) of two simple
   hypotheses regarding the Levy-Khintchine triplet of a wide family of
   Levy processes is analyzed: we concentrate on continuous paths and pure
   increasing jump Levy processes. Appealing to the theory of Markov
   processes, we employ a general method for determining the stopping
   boundaries and the expected length of the SPRT for a given admissible
   pair (alpha, beta) of error probabilities. The well-known results of the
   Wiener and Poisson sequential testing can be derived accordingly. The
   explicit solution for the SPRT of two simple hypotheses about the
   parameter p is an element of (0, 1) of a Levy negative binomial process
   is shown.}},
