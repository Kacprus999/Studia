@inproceedings{ WOS:000426426300075,
Author = {Dyskin, Arcady and Pasternak, Elena},
Editor = {{Papamichos, E and Papanastasiou, P and Pasternak, E and Dyskin, A}},
Title = {{Instability of Geomaterials Caused by Transitional Negative Stiffness}},
Booktitle = {{BIFURCATION AND DEGRADATION OF GEOMATERIALS WITH ENGINEERING
   APPLICATIONS}},
Series = {{Springer Series in Geomechanics and Geoengineering}},
Year = {{2017}},
Pages = {{599-604}},
Note = {{11th International Workshop on Bifurcation and Degradation in
   Geomaterials (IWBDG), Limassol, CYPRUS, MAY 21-25, 2017}},
Organization = {{Univ Cyprus; Aristotle Univ Thessaloniki; Univ Western Australia; Cyprus
   Tourism Org; ALERT Geomaterials; SINTEF Petr Res; MTS Syst;
   Themeliotechniki Cyprus Ltd; Dreamscape Conf}},
Abstract = {{A mechanism of instability of geomaterials in compression is proposed
   based on the phenomenon of transitional negative stiffness associated
   with the process of crack/fracture formation. This phenomenon acts only
   for a very short time as the fracture is being formed. The negative
   stiffness cracks increase the Poisson's ratio and thus are capable of
   making nearly incompressible material elastically unstable. A
   geomaterial can reach the limiting value of the incremental Poisson's
   ratio due to dilatancy observed in compression. While dilatancy is a
   non-linear phenomenon, incrementally, at the time of fracture formation
   it is elastic and hence can be made unstable by the transitional
   negative stiffness.}},
