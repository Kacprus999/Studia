@article{ WOS:000254229500020,
Author = {Narojczyk, J. W. and Wojciechowski, K. W.},
Title = {{Elastic properties of the fcc crystals of soft spheres with size
   dispersion at zero temperature}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2008}},
Volume = {{245}},
Number = {{3}},
Pages = {{606-613}},
Month = {{MAR}},
Note = {{1st International Conference on Auxetics and Anomalous Systems, Univ
   Exeter, Exeter, ENGLAND, SEP 04-06, 2006}},
Abstract = {{Elastic constants of soft sphere systems, interacting through
   n-inverse-power potential and exhibiting aperiodicity, caused by the
   size dispersion of the spheres, are determined at zero temperature by
   computer simulations. It is shown that, at a fixed number density of the
   spheres, the Poisson's ratio, as welt as other elastic constants,
   increase with increasing disorder in the system. Properties of two types
