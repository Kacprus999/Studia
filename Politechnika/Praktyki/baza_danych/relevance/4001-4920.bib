@article{ WOS:000234339900015,
Author = {Bezazi, A and Scarpa, F and Remillat, C},
Title = {{A novel centresymmetric honeycomb composite structure}},
Journal = {{COMPOSITE STRUCTURES}},
Year = {{2005}},
Volume = {{71}},
Number = {{3-4}},
Pages = {{356-364}},
