@article{ WOS:000472990900055,
Author = {Thomas, T. and Tiwari, G.},
Title = {{Energy absorption and in-plane crushing behavior of aluminium reinforced
   honeycomb}},
Journal = {{VACUUM}},
Year = {{2019}},
Volume = {{166}},
Pages = {{364-369}},
Month = {{AUG}},
Note = {{International Symposium on Functional Materials (ISFM) - Energy and
   Biomedical Applications, Chandigarh, INDIA, APR 13-15, 2018}},
Abstract = {{Aluminium honeycomb is a lightweight load carrying structure widely used
   in aerospace and automotive industries. In this study, In-plane
   compression analysis of AA 8011 reinforced honeycomb was performed
   numerically and experimentally. Geometrical parameters like cell size,
   cell wall thickness and node length were varied for attaining different
   models. Cell size was varied as 7 mm and 10 mm while node length as 25
   mm, 30 mm and 35 mm respectively. Cell wall thickness of reinforced
   honeycomb was kept as 0.1, 0.2 and 0.3 mm respectively. Finite element
   simulations were performed on various combination of reinforced
   honeycomb configuration using IS DYNA. Numerical model was validated
   with experimental results performed on reinforced core. Cell-wall
   thickness has major influence on energy absorption and peak crushing
   force of honeycomb. Enhancement in cell wall thickness increased
   crushing strength and energy absorption capacity of the structure
   whereas reverse effect was observed with increase in cell size. Node
   length has less impact on performance characteristics of honeycomb core.}},
