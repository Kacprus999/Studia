@article{ WOS:000295459800002,
Author = {Bhuiyan, Lutful Bari and Henderson, Douglas},
Title = {{Evidence for a second, local contact condition for a planar electric
   double layer containing size and valency asymmetric ions}},
Journal = {{MOLECULAR PHYSICS}},
Year = {{2011}},
Volume = {{109}},
Number = {{15}},
Pages = {{1863-1871}},
Abstract = {{A generalised form of a local contact condition for the charge profile
   in a primitive model planar double layer {[}Bhuiyan, Outhwaite, and
   Henderson, Mol. Phys. 107, 343 (2009)] at low electrode charge is
   examined for completely asymmetric, binary electrolytes. The cation and
   anion sizes are taken to be different from each other with the valencies
   being 2(+):1(-) or 1(+):2(-), while the electrode surface charge density
   is varied from being negative through zero to being positive. Monte
   Carlo simulation data obtained for such double layer systems at varying
   ionic radius ratios and electrolyte concentrations suggest the
   generalised contact relation to be valid at low charge on the electrode.}},
