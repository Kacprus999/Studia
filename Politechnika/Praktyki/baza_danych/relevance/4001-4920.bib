Author = {Topolov, Vitaly Yu and Krivoruchko, Andrey V. and Bowen, Christopher R.},
Title = {{Anisotropy of electromechanical properties and hydrostatic response of
   advanced 2-2-type composites}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLICATIONS AND MATERIALS SCIENCE}},
Year = {{2012}},
Volume = {{209}},
Number = {{7}},
