Author = {Twarek, LM and Haack, SJ and Flatau, AB},
Editor = {{Lagoudas, DC}},
Title = {{Design and modeling of a single-crystal iron-gallium large
   magnetostrictive dynamic research transducer}},
Booktitle = {{SMART STRUCTURES AND MATERIALS 2004: ACTIVE MATERIALS: BEHAVIOR AND
   MECHANICS}},
Series = {{PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)}},
Year = {{2004}},
