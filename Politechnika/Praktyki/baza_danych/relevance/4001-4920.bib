@inproceedings{ WOS:000398997003107,
Author = {Collet, Manuel and Ouisse, Morvan},
Editor = {{Crocker, MJ and Pawelczyk, M and Pedrielli, F and Carletti, E and Luzzi, S}},
Title = {{SMART METACOMPOSITES FOR THE SYNTHESIS OF VIBROA-COUSTICAL PROPERTIES}},
