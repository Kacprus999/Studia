@inproceedings{ WOS:000392489200039,
Author = {Coleman, Matthew P. and Schaffer, Les},
Book-Group-Author = {{Int Inst Acoust \& Vibrat}},
Title = {{A DEGENERATE CASE OF THE TIMOSHENKO BEAM PROBLEM: AN ASYMPTOTIC AND
   NUMERICAL STUDY}},
Booktitle = {{PROCEEDINGS OF THE 17TH INTERNATIONAL CONGRESS ON SOUND AND VIBRATION}},
Year = {{2010}},
Note = {{17th International Congress on Sound and Vibration (ICSV), Cairo, EGYPT,
   JUL 18-22, 2010}},
Organization = {{Int Inst Acoust \& Vibrat; Acoust Soc Egypt; Ain Shams Univ; Nile Univ;
   Int Union Theoret \& Appl Mech; Amer Soc MechEngineers Int; Inst Mech
   Engineers}},
Abstract = {{Recently, the Timoshenko beam, and generalizations thereof, have been
   considered as models for the vibration of single and multiple walled
   carbon nanotubes. Further, it has been shown that, in some instances,
   nanotubes and nanotube sheets possess the remarkable property that their
   Poisson's ratio is negative. In these cases, the degenerate case of the
   Timoshenko beam problem, where E = k'G, becomes physically realizable.
   The asymptotic derivation of the vibration spectrum in this case is
   qualitatively different from that for the non-degenerate case. We
   perform such a derivation, and we compare the asymptotic results with
   numerical results for values of the parameters for which E approaches
   k'G. The results suggest that the degenerate case is robust, in an
   asymptotic sense.}},
