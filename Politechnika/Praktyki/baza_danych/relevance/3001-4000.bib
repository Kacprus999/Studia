@article{ WOS:000642405100010,
Author = {Fu, Xi and Yang, Houyong and Fu, Ling and He, Chaozheng and Huo, Jinrong
   and Guo, Jiyuan and Li, Liming},
Title = {{Prediction of semiconducting SiP2 monolayer with negative Possion's
   ratio, ultrahigh carrier mobility and CO2 capture ability}},
