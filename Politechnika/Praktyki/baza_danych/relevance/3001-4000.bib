@article{ WOS:A1992JA90000018,
Author = {PUCK, A},
Title = {{SHOULD FIBER-PLASTICS COMPOSITES BE DESIGNED WITH STRAIN OR STRESS
   CRITERIA}},
Journal = {{KUNSTSTOFFE-GERMAN PLASTICS}},
Year = {{1992}},
Volume = {{82}},
Number = {{5}},
Pages = {{431-434}},
Month = {{MAY}},
Abstract = {{In laminates, the UD layers are subject to multi-axial stress. Crack
   formation in the form of inter-fibre fractures caused by transverse
   stresses then occurs in UD-GRP layers at approximately the same
   transverse stresses, but highly varying strains transverse to the fibre
   direction. The latter range from slightly negative values up to three
   times the monoaxially measured elongation at break. This is a
   consequence of the transverse contraction effect (Poisson's ratio), as a
   result of which strain-fracture criteria become complicated and obscure.
   Stress criteria are more meaningful and therefore better suited for
   design practice.}},
