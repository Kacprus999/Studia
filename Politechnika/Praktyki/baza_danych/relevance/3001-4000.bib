@article{ WOS:000264342300030,
Author = {Aliev, Ali E. and Oh, Jiyoung and Kozlov, Mikhail E. and Kuznetsov,
   Alexander A. and Fang, Shaoli and Fonseca, Alexandre F. and Ovalle,
   Raquel and Lima, Marcio D. and Haque, Mohammad H. and Gartstein, Yuri N.
   and Zhang, Mei and Zakhidov, Anvar A. and Baughman, Ray H.},
Title = {{Giant-Stroke, Superelastic Carbon Nanotube Aerogel Muscles}},
Journal = {{SCIENCE}},
Year = {{2009}},
Volume = {{323}},
Number = {{5921}},
Pages = {{1575-1578}},
Month = {{MAR 20}},
Abstract = {{Improved electrically powered artificial muscles are needed for
   generating force, moving objects, and accomplishing work. Carbon
   nanotube aerogel sheets are the sole component of new artificial muscles
   that provide giant elongations and elongation rates of 220\% and (3.7 x
   10(4))\% per second, respectively, at operating temperatures from 80 to
   1900 kelvin. These solid-state-fabricated sheets are enthalpic rubbers
   having gaslike density and specific strength in one direction higher
   than those of steel plate. Actuation decreases nanotube aerogel density
   and can be permanently frozen for such device applications as
   transparent electrodes. Poisson's ratios reach 15, a factor of 30 higher
   than for conventional rubbers. These giant Poisson's ratios explain the
   observed opposite sign of width and length actuation and result in rare
   properties: negative linear compressibility and stretch densification.}},
Publisher = {{AMER ASSOC ADVANCEMENT SCIENCE}},
Address = {{1200 NEW YORK AVE, NW, WASHINGTON, DC 20005 USA}},
Type = {{Article}},
