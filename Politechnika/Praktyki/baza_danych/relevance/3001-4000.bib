@article{ WOS:000223801100026,
Author = {Cernoch, T and Landa, M and Novak, V and Sedlak, P and Sittner, P},
Title = {{Acoustic characterization of the elastic properties of austenite phase
   and martensitic transformations in CuAlNi shape memory alloy}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2004}},
Volume = {{378}},
Number = {{1-2}},
Pages = {{140-144}},
Month = {{SEP 22}},
Note = {{9th International Symposium on Physics of Materials, Charles Univ, Fac
   Math \& Phys, Prague, CZECH REPUBLIC, SEP 01-04, 2003}},
Organization = {{Charles Univ, Dept Met Phys}},
Abstract = {{A complete set of second and third order elastic constants of the cubic
   austenite phase in CuA/Ni shape memory alloy was determined by
   ultrasonic pulse-echo method. Because of the very high elastic
   anisotropy of the austenite phase (A = 12.02), a negative Poisson ratio
   V is obtained for loading the crystal in certain directions, e.g. nu(12)
   = nu(21) = -0.62 in the coordinate system {[}110], {[}110], {[}001].
   Measurements of the third order elastic constants were carried out in
   nine selected specimen geometries (acoustic modes). The wave velocity
   was highly sensitive to the applied stress only in two shear modes. The
   onset of the stress-induced martensitic transformation (MT) was detected
   by acoustic emission (AE), ultrasonic wave velocity and attenuation
   measurements in given order. The acoustic emission was thus the most
   sensitive acoustic method for detection of early stages of the
   transformation. (C) 2004 Elsevier B.V. All rights reserved.}},
