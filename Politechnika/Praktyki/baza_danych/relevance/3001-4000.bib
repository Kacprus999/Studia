@article{ WOS:A1992JJ27500012,
Author = {REY, JC and MUNOZCHAPULI, R},
Title = {{INTRASPECIFIC AND INTERSPECIFIC ASSOCIATION OF LARGE PELAGIC FISHES
   INFERRED FROM CATCH DATA OF SURFACE LONGLINE}},
Journal = {{ENVIRONMENTAL BIOLOGY OF FISHES}},
Year = {{1992}},
Volume = {{35}},
Number = {{1}},
Pages = {{95-103}},
Month = {{SEP}},
Abstract = {{This paper analyses the distribution of longline catches of swordfish,
   Xiphias gladius, and six species of pelagic elasmobranchs, during a
   fishing voyage in the tropical eastern Atlantic. Seven scales of
   sampling units, ranging from about 0.72 to 7.2 km were used. Observed
   frequencies of appearance of each species in the basic sampling unit,
   were compared with theoretical frequencies obtained from the Poisson and
   negative binomial distributions. For the study of the interspecific
   relationships. contingency tables were used and the degree of
   association was estimated using the chi-2-test and the coefficient v of
   Kendall. The catch distribution of the swordfish and the devil ray
   (Mobula sp.) can be explained by both randomness and a slight trend to
   association. For the pelagic sharks, a clear trend towards intraspecific
   clustering was found. Catches of two species of the genus Carcharhinus
   showed the most positive trends to interspecific association, especially
   in short ranges, probably due to attraction to previously hooked fish.
   The possible causes of these distributions are discussed.}},
