@article{ WOS:000407907800003,
Author = {Jiang, Jin-Wu},
Title = {{An empirical description for the hinge-like mechanism in single-layer
   black phosphorus: The angle-angle cross interaction}},
Journal = {{ACTA MECHANICA SOLIDA SINICA}},
Year = {{2017}},
Volume = {{30}},
Number = {{3}},
Pages = {{227-233}},
Month = {{JUN}},
Abstract = {{The single-layer black phosphorus is characterized by its puckered
   configuration that possesses the hinge-like behavior, which leads to the
   highly anisotropic in-plane Poisson's ratios and the negative
   out-of-plane Poisson's ratio. We demonstrate that the hinge-like
   mechanism can be described by the angle-angle cross interaction, which,
   combined with the bond stretching and angle bending interactions, is
   able to provide a good description for the mechanical properties of
   single-layer black phosphorus. We also propose a nonlinear angle-angle
   cross interaction, which follows the form of Stillinger-Weber potential
   and can be advantageous for molecular dynamics simulations of
   single-layer black phosphorus under large deformation. (C) 2017
   Published by Elsevier Ltd on behalf of Chinese Society of Theoretical
