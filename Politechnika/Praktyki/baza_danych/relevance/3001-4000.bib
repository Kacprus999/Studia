@article{ WOS:000461236600017,
Author = {Zhou, Xiaoqin and Yang, Lu and Ma, Meng and Zhang, Lei},
Title = {{Negative Compressibility of 3D Cellular Models Constructed by Hinging
   Hexagonal Truss Mechanism}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2019}},
Volume = {{256}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{Three 3D models with negative compressibility have been presented in our
   previous paper {[}X. Q. Zhou et al., Phys. Status Solidi B 2016, 253,
   1977]. However, the 2D mechanism (i.e., wine-rack mechanism) which we
   used to design 3D models is so symmetrical that the axial properties of
   it are equivalent, so we have only focused on geometry features. In this
   work, an extended study is conducted in order to find a more efficient
   method to design 3D structures with evident negative compressibility.
   Through theoretical modeling, the compressibility properties of three 3D
   models made from hinging hexagonal truss mechanism are analyzed and the
   conditions for negative compressibility to be exhibited are discussed.
   The study shows that in addition to the geometry features, negative
   compressibility effects can also be significantly affected by the
   arrangement of the framework and layout orientations of 2D mechanism.}},
