@article{ WOS:000379027100003,
Author = {Ding, Yaoyu and Kovacevic, Radovan},
Title = {{Feasibility Study on 3-D Printing of Metallic Structural Materials with
   Robotized Laser-Based Metal Additive Manufacturing}},
Journal = {{JOM}},
Year = {{2016}},
Volume = {{68}},
Number = {{7}},
Pages = {{1774-1779}},
Month = {{JUL}},
Abstract = {{Metallic structural materials continue to open new avenues in achieving
   exotic mechanical properties that are naturally unavailable. They hold
   great potential in developing novel products in diverse industries such
   as the automotive, aerospace, biomedical, oil and gas, and defense.
   Currently, the use of metallic structural materials in industry is still
   limited because of difficulties in their manufacturing. This article
