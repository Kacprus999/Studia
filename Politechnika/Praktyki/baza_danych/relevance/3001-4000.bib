@article{ WOS:000402058900016,
Author = {Jiao, Jiantao and Venkat, Kartik and Weissman, Tsachy},
Title = {{Relations Between Information and Estimation in Discrete-Time Levy
   Channels}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{2017}},
Volume = {{63}},
Number = {{6}},
Pages = {{3579-3594}},
Month = {{JUN}},
Note = {{IEEE International Symposium on Information Theory (ISIT), Honolulu, HI,
   JUN 29-JUL 04, 2014}},
Organization = {{IEEE; IEEE Informat Theory Soc; NSF; Qualcomm; IBM Res; LG}},
Abstract = {{Fundamental relations between information and estimation have been
   established in the literature for the discrete-time Gaussian and Poisson
   channels. In this paper, we demonstrate that such relations hold for a
   much larger class of observation models. We introduce the natural family
   of discrete-time Levy channels where the distribution of the output
   conditioned on the input is infinitely divisible. For Levy channels, we
   establish new representations relating the mutual information between
   the channel input and output to an optimal expected estimation loss,
   thereby unifying and considerably extending results from the Gaussian
   and Poisson settings. We demonstrate the richness of our results by
   working out two examples of Levy channels, namely the gamma channel and
   the negative binomial channel, with corresponding relations between
   information and estimation. Extensions to the setting of mismatched
   estimation are also presented.}},
