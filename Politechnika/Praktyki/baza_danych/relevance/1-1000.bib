@article{ WOS:000368744000030,
Author = {Pasternak, Elena and Shufrin, Igor and Dyskin, Arcady V.},
Title = {{Thermal stresses in hybrid materials with auxetic inclusions}},
Journal = {{COMPOSITE STRUCTURES}},
Year = {{2016}},
Volume = {{138}},
Pages = {{313-321}},
Month = {{MAR 15}},
Abstract = {{We study thermo-elastic behaviour of the hybrid materials with auxetic
   inclusions. To this end we analyse representative volume elements
   consisting of a large number of randomly positioned cubic inclusions
   using the finite element method and determine the effective coefficient
   of thermal expansion and the average thermal stresses in constrained
   hybrid materials developed as a result of a uniform temperature change.
   We study the effect of Poisson's ratio, Young's modulus and the
   coefficient of thermal expansion of the inclusions on the effective
   coefficient of thermal expansion and thermal stresses. We demonstrate
   that the auxeticity of the inclusions can reduce thermal stresses, which
   is controlled by the values of the Poisson's ratio and the coefficients
   of thermal expansion of the inclusions and matrix. (C) 2015 Elsevier
   Ltd. All rights reserved.}},
