@article{ WOS:000445315900037,
Author = {Goto, Keita and Arai, Masahiro and Matsuda, Tetsuya and Kubo, Gai},
Title = {{Elasto-viscoplastic analysis for negative through-the-thickness
   Poisson's ratio of woven laminate composites based on homogenization
   theory}},
