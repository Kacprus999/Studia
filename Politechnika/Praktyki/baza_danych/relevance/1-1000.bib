@article{ WOS:000296382600009,
Author = {Kim, Dae-Yoon and Kim, Namil and Hwang, Seok-Ho and Kuo, Shiao-Wei and
   Lee, Jong-Hoon and Lee, Sang-A and Lee, Joong Hee and Nah, Changwoon and
   Jeong, Kwang-Un},
Title = {{Poisson's Ratios of Honeycomb-Structured Polymer Sheets Under Large
   Deformation}},
Journal = {{MACROMOLECULAR CHEMISTRY AND PHYSICS}},
Year = {{2011}},
Volume = {{212}},
Number = {{20}},
Pages = {{2275-2280}},
Month = {{OCT 17}},
Abstract = {{The mechanical properties of 2D honeycomb-structured polymer sheets
   (HSPS) are investigated with respect to the polar angle at large
   deformation. The tensile force fluctuates as an undamped negative
   sinusoidal wave with a periodicity of 15 degrees. The amplitude of the
   fluctuation is proportional to the thickness of the HSPS, but the
   Young's modulus is independent of the thickness and the polar angle.
   Total and local Poisson's ratios of HSPS at the 30 degrees polar angle
   are lower than those obtained at the 0 degrees polar angle over the
   entire strain range, and the difference becomes smaller with increasing
   tensile strain. The experimentally observed structure-property
   relationship of the HSPS can provide important clues for understanding
   the deformation behavior of patterned polymeric materials.}},
