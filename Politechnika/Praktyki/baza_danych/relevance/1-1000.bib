@article{ WOS:000261088100012,
Author = {Shilko, S. V. and Petrokovets, E. M. and Pleskachevsky, Yu. M.},
Title = {{Prediction of auxetic phenomena in nanoporomaterials}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2008}},
Volume = {{245}},
Number = {{11, SI}},
Pages = {{2445-2453}},
Month = {{NOV}},
Abstract = {{Porous materials are efficient structures in respect to optimizing
   strength and stiffness for a given weight. A new means of improving the
   characteristics of the samples may be realized using abnormal
   deformation properties of auxetic porous materials having a negative
   Poisson's ratio v. Auxetic porous materials are suitable for creating
   adaptive contact joints and for replacing damaged biotissue. It is known
   that the inverted or re-entrant cell structure of porous auxetics may be
   obtained by isotropic permanent volumetric compression of conven(t)ional
   foam, resulting in microbuckling of the cell walls. There is interest in
   compression-driven self-assembly as a means to create auxetic porous
   structures on a nanoscale. To predict the deformation behaviour of
   porous materials under various types of loading, corresponding
   mathematical models should be developed. The possibility of
   compression-driven self-assembly of the auxetic nanostructures has been
   predicted. (C) 2008 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim}},
