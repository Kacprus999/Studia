@article{ WOS:000455348100010,
Author = {Park, Harold S. and Kim, Sung Youb},
Title = {{A perspective on auxetic nanomaterials}},
Journal = {{NANO CONVERGENCE}},
Year = {{2017}},
Volume = {{4}},
Abstract = {{Nanomaterials have recently been found to exhibit auxetic behavior, or a
   negative Poisson's ratio, whereby the lateral dimensions of the material
   expand, rather than shrink, in response to applied tensile loading. In
