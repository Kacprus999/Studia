@article{ WOS:000189315400013,
Author = {Yang, X and Yang, MB and Li, ZM and Feng, JM},
Title = {{The negative poisson's ratio effect of polyolefin blends}},
Journal = {{ACTA POLYMERICA SINICA}},
Year = {{2003}},
Number = {{2}},
Pages = {{221-224}},
Month = {{APR}},
Abstract = {{In recent years, materials with negative poisson's ratio properties have
   attracted great interest from both scientific and industry fields, since
   they exhibit special properties: such as dramatic increases in the cross
   section when a force is applied in the vertical direction and
   contraction when compacted. In this paper, polyolefin blends with
   negative poisson's ratios were prepared successfully. The value of the
   negative poisson's ratio of the polyolefin blends was investigated by
   using a YJY stress flake deformation transducer. A maximum value of - 9
   was observed for the poisson's ratio of the blends, and the stable value
   for samples may reach - 1.4. The morphology of the blends was also
   studied. The results showed that the convacation phenomenon occurred
   between the two components when the samples were stretched. It also
   showed that it is possible to get materials with negative poisson's
   ratio by decreasing the compatibility of the two components in the
   blends.}},
