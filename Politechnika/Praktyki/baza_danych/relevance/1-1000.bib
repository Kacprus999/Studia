@article{ WOS:000647965300001,
Author = {Kuznetsov, Sergey V.},
Title = {{Stoneley waves in mild auxetics}},
Journal = {{WAVES IN RANDOM AND COMPLEX MEDIA}},
Abstract = {{The current research concerns with the analysis of Stoneley waves
   propagation along an interface between two dissimilar homogeneous and
   isotropic halfspace having negative Poisson's ratios (auxetics). The
   main attention is paid to the case when both contacting media satisfy
   the Wiechert condition ensuring the existence of Stoneley wave. Closed
   form expressions are derived for derivatives of the Stoneley secular
   equation. The velocity of Stoneley waves is analyzed both theoretically
   and numerically in a broad range of the Wiechert parameter variation.}},
