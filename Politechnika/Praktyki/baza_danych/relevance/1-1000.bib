@article{ WOS:000513774700002,
Author = {Wang, Yingjun and Liao, Zhongyuan and Shi, Shengyu and Wang, Zhenpei and
   Poh, Leong Hien},
Title = {{Data-Driven Structural Design Optimization for Petal-Shaped Auxetics
   Using Isogeometric Analysis}},
Journal = {{CMES-COMPUTER MODELING IN ENGINEERING \& SCIENCES}},
Year = {{2020}},
Volume = {{122}},
Number = {{2}},
Pages = {{433-458}},
Abstract = {{Focusing on the structural optimization of auxetic materials using
   data-driven methods, a back-propagation neural network (BPNN) based
   design framework is developed for petal-shaped auxetics using
   isogeometric analysis. Adopting a NURBS-based parametric modelling
