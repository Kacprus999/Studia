@article{ WOS:000233065500013,
Author = {Ting, TCT and Barnett, DM},
Title = {{Negative Poisson's ratios in anisotropic linear elastic media}},
Journal = {{JOURNAL OF APPLIED MECHANICS-TRANSACTIONS OF THE ASME}},
Year = {{2005}},
Volume = {{72}},
Number = {{6}},
Pages = {{929-931}},
Month = {{NOV}},
Abstract = {{Poisson's ratio for an anisotropic linear elastic material depends on
   two orthogonal directions n and m. Materials with negative Poisson
   ratios for all (n, m) pairs are called completely auxetic while those
   with positive Poisson's ratios for all (n, m) pairs are called
   nonauxetic. Simple necessary and sufficient conditions on elastic
   compliances are derived to identify if any given material of cable or
   hexagonal symmetry is completely auxetic or nonauxetic. When these
   conditions are not satisfied, the medium is auxetic for some (n, m)
   pairs. Several simple necessary conditions for completely auxetic or
   nonauxetic media are derived for a general anisotropic elastic material.}},
