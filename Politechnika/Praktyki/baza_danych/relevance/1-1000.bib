@inproceedings{ WOS:000228398300014,
Author = {Alderson, A and Alderson, KL and Evans, KE and Grima, JN and Williams, M},
Editor = {{Gupta, M and Lim, CYH}},
Title = {{Modelling of negative Poisson's ratio nanomaterials: Deformation
   mechanisms, structure-property relationships and applications}},
