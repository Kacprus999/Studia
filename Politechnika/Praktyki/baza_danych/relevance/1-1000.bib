@article{ WOS:000555129600001,
Author = {Kuznetsov, Sergey V.},
Title = {{Stoneley waves in auxetics and non-auxetics: Wiechert case}},
Journal = {{MECHANICS OF ADVANCED MATERIALS AND STRUCTURES}},
Abstract = {{An interesting phenomenon observed in auxetics (materials with negative
   Poisson's ratio) relates to appearing anomalous velocity intersections
   of the interfacial Stoneley waves at varying acoustic properties of the
   contacting media satisfying Wiechert condition. Comparative analysis
   with the non-auxetics (materials with positive Poisson's ratio) revealed
   the absence of such intersections. The analysis is based on solving
   secular equation for Stoneley waves in the modified Scholte form.}},
