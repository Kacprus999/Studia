@article{ WOS:000490786800008,
Author = {Reid, Daniel R. and Pashine, Nidhi and Bowen, Alec S. and Nagel, Sidney
   R. and de Pablo, Juan J.},
Title = {{Ideal isotropic auxetic networks from random networks}},
Journal = {{SOFT MATTER}},
Year = {{2019}},
Volume = {{15}},
Number = {{40}},
Pages = {{8084-8091}},
Month = {{OCT 28}},
Abstract = {{Auxetic materials are characterized by a negative Poisson's ratio, nu.
   As the Poisson's ratio approaches the lower isotropic mechanical limit
   of nu = -1, materials show enhanced resistance to impact and shear,
   making them suitable for applications ranging from robotics to impact
   mitigation. Past experimental efforts aimed at reaching the nu = -1
   limit have resulted in highly anisotropic materials, which show a
   negative Poisson's ratio only when subjected to deformations along
   specific directions. Isotropic designs have only attained moderately
   auxetic behavior or have led to solutions that cannot be manufactured in
   3D. Here, we present a design strategy to create isotropic structures
   from disordered networks, which result in Poisson's ratios as low as nu
   = -0.98. The materials conceived through this approach are successfully
   fabricated in the laboratory and behave as predicted. nu depends on
   network structure and bond strengths; this sheds light on the motifs
   which lead to auxetic behavior. The ideas introduced here can be
   generalized to 3D, a wide range of materials, and a spectrum of length
   scales, thereby providing a general platform that could impact
   technology.}},
