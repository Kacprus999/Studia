@article{ WOS:000325447700015,
Author = {Grima, Joseph N. and Cauchi, Reuben and Gatt, Ruben and Attard, Daphne},
Title = {{Honeycomb composites with auxetic out-of-plane characteristics}},
Journal = {{COMPOSITE STRUCTURES}},
Year = {{2013}},
Volume = {{106}},
Pages = {{150-159}},
Month = {{DEC}},
Abstract = {{Systems with a negative Poisson's ratio (auxetic) exhibit the unusual
   yet very useful property of getting wider rather than thinner when
   uniaxially stretched. A novel mechanism to generate auxetic behaviour at
   tailor-made values which may be implemented in composites manufacture
   using readily available materials is proposed. FEA simulations are used
   to provide a proof of principle for this concept and an analytical model
   is proposed so as to elucidate the requirements for auxetic behaviour.
   It is shown that the simulations and analytical model give comparable
   results and confirm that this system may reach giant negative values of
   the Poisson's ratio. (C) 2013 Elsevier Ltd. All rights reserved.}},
