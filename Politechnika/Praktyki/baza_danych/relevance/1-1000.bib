@article{ WOS:000491730100001,
Author = {Chen, Junli and Du, Zhaoqun and Li, Tianyuan},
Title = {{Structural design and characterization of highly elastic woven fabric
   containing helical auxetic yarns}},
Journal = {{TEXTILE RESEARCH JOURNAL}},
Year = {{2020}},
Volume = {{90}},
Number = {{7-8}},
Pages = {{809-823}},
Month = {{APR}},
Abstract = {{Auxetic textiles have been the focus of much attention due to their
   great promise for advanced protective clothing, flexible energy harvest
   devices, and functional textiles. Herein, plain fabric, basket fabric,
   and a derivative weave with the warp and weft yarns arrangement in a
   series of zigzags were prepared by incorporating different initial wrap
   density helical auxetic yarns in the weft direction using a commercial
   semi-automatic loom. The derivative weave using HAYs with a 150 m(-1)
   initial wrap density as the weft yarn not only possesses superior
   auxetic behavior but also has good performance in strength and
   elasticity-essential properties useful for textile daily application.
   This fabric exhibits a high auxetic effect (nu = -0.585), low elastic
   deformation (total deformation of 8.4\% at 20\% strain), excellent
