@article{ WOS:000244804200021,
Author = {Shang Xinchun and Lakes, Roderic S.},
Title = {{Stability of elastic material with negative stiffness and negative
   Poisson's ratio}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC SOLID STATE PHYSICS}},
Year = {{2007}},
Volume = {{244}},
Number = {{3, SI}},
Pages = {{1008-1026}},
Month = {{MAR}},
Note = {{2nd Workshop on Auxetics and Related Systems, Bedlewo, POLAND, AUG
   19-23, 2005}},
Abstract = {{Stability of cuboids and cylinders of an isotropic elastic material with
   negative stiffness under partial constraint is analyzed using an
   integral method and Rayleigh quotient. It is not necessary that the
   material exhibit a positive definite strain energy to be stable. The
   elastic object under partial constraint may have a negative bulk modulus
   K and yet be stable. A cylinder of arbitrary cross section with the
   lateral surface constrained and top and bottom planar surfaces is stable
   provided the shear modulus G > 0 and - G/3 < K < 0 or K > 0. This
   corresponds to an extended range of negative Poisson's ratio, - infinity
   < v < - 1. A cuboid is stable provided each of its surfaces is an
   aggregate of regions obeying fully or partially constrained boundary
   conditions. (C) 2007 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim.}},
