@article{ WOS:000401631700003,
Author = {Zeng, Jifang and Hu, Hong and Zhou, Lin},
Title = {{A study on negative Poisson's ratio effect of 3D auxetic orthogonal
   textile composites under compression}},
Journal = {{SMART MATERIALS AND STRUCTURES}},
Year = {{2017}},
Volume = {{26}},
Number = {{6}},
Month = {{JUN}},
Abstract = {{More and more researches have been focused on auxetic composite
   materials and a number of composite structures have been fabricated,
   synthesized or theoretically predicted. Since their structures are
   complex, their mechanical behavior is very difficult to be
   characterized. The purpose of the present paper is to systematically
   investigate the negative Poisson's ratio effect of a novel
   three-dimensional auxetic orthogonal textile composite under
   compression. Firstly, a set of equations are derived for the theoretical
   calculation of the Poisson's ratio of the composite under uniaxial
   compression via an analytical analysis. Secondly, a finite element model
   (FEM) is created by ANSYS Parameter Design Language and is verified by
   experiment. The deviation between the simulation and experimental
   results are carefully discussed. Thirdly, the effects of geometry
   parameters and material properties on the negative Poisson's ratio
   behavior of the composite are discussed based on the FEM simulated
   results. At last, a general basis is concluded. It is expected that the
   outcomes of this study could be useful to guide the design and
   fabrication of auxetic textile composites with required negative
   Poisson's ratio behavior.}},
