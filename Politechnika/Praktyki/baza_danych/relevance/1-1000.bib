@inproceedings{ WOS:000355991300016,
Author = {Mukherjee, Sushovan and Scarpa, Fabrizio and Gopalakrishnan, S.},
Editor = {{Varadan, VK}},
Title = {{Novel Design of Honeycombs Using a Seamless Combination of Auxetic and
   Conventional Cores Toward Phononic Band Gap Engineering}},
Booktitle = {{NANOSENSORS, BIOSENSORS, AND INFO-TECH SENSORS AND SYSTEMS 2015}},
