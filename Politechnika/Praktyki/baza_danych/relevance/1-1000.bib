@article{ WOS:000075831200002,
Author = {Wei, GY and Edwards, SF},
Title = {{Auxeticity windows for composites}},
Journal = {{PHYSICA A}},
Year = {{1998}},
Volume = {{258}},
Number = {{1-2}},
Pages = {{5-10}},
Month = {{SEP 1}},
Abstract = {{Materials with negative Poisson ratios are known to have high shear
   rigidities - a useful property for many types of structural and
   functional materials. To improve upon relatively low Young's modulus of
   existing auxetics, one may consider embedding such components in an
   elastic material with sufficiently high modulus. We show theoretically
   that such composite materials do exhibit auxeticity when the inclusion
   volume fraction exceeds a critical value and the ratio of Young's
   modulus of inclusion to that of a matrix falls within a definite
   interval. The existence of these auxeticity windows, once verified
   experimentally, opens up a new avenue of auxetics research. (C) 1998
   Elsevier Science B.V. All rights reserved.}},
