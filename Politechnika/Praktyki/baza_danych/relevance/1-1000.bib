@article{ WOS:000613223200015,
Author = {Chen, Ming-Ming and Fu, Ming-Hui and Lan, Lin-Hua and Sheshenin, Sergey
   V.},
Title = {{A novel 3D structure with tunable Poisson's ratio and tailorable
   coefficient of thermal expansion based on a tri-material triangle unit}},
Journal = {{COMPOSITE STRUCTURES}},
Year = {{2020}},
Volume = {{253}},
Month = {{DEC 1}},
Abstract = {{Auxetic materials and negative thermal expansion materials show great
   application potential in many important fields. In this paper, a novel
   three-dimensional truss structure with a tunable Poisson's ratio and
   tailorable coefficient of thermal expansion was developed. Since the
   deformation of each rod was dominated by axial tension or compression,
   the structure showed high stiffness. Equations regarding equivalent
   elasticity parameters and coefficients of thermal expansion of the
   structure were obtained by displacement and unit force methods. A
   numerical simulation verification was performed by ANSYS 16.0 software.
   The verification results indicated that the formulas have high precision
